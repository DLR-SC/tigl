            <transformation>
              <translation>
                <x>1</x>
              </translation>
              <scaling>
                <x>1.0</x>
                <y>0.5</y>
                <z>0.5</z>
              </scaling>
            </transformation>
            <sections>
              <section uID="fuselage_1Section1ID">
                <name>Fuselage Section 1</name>
                <transformation />
                <elements>
                  <element uID="fuselage_1Section1IDElement1">
                    <name>Fuselage Section 1</name>
                    <profileUID>fuselageCircleProfileuID</profileUID>
                    <transformation />
                  </element>
                </elements>
              </section>
              <section uID="fuselage_1Section2ID">
                <name>Fuselage Section 2</name>
                <transformation>
                  <translation refType="absLocal">
                    <x>0.5</x>
                  </translation>
                </transformation>
                <elements>
                  <element uID="fuselage_1Section2IDElement1">
                    <name>Fuselage Section 2</name>
                    <profileUID>fuselageCircleProfileuID</profileUID>
                    <transformation />
                  </element>
                </elements>
              </section>
              <section uID="fuselage_1Section3ID">
                <name>Fuselage Section 3</name>
                <transformation />
