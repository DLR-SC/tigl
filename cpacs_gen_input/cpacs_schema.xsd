	<xsd:complexType name="materialDefinitionForProfileBasedType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of the material properties of the structural
							profile sheet.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>MaterialDefinitionForProfileBased type, containing a
							material definition (Reference to material and thickness) for
							profile based objects.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="sheetUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>uID of the sheet to which the material
								properties shall be applied.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:sequence>
							<xsd:element name="compositeUID" type="stringUIDBaseType">
								<xsd:annotation>
									<xsd:documentation>uID of a composite definition.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="orthotropyDirection" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Orthoropy direction of the composite.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="thicknessScaling" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Scaling factor of the composite thickness.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element name="materialUID" type="stringUIDBaseType">
								<xsd:annotation>
									<xsd:documentation>uID of a material definition.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="thickness" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Absolute thickness of the material.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="materialDefinitionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>materialDefinitionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>MaterialDefinition type, containing a material
							definition (Reference to material and thickness)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:annotation>
						<xsd:documentation>choice between composite / isotropic material
							definition</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="compositeUID" type="stringUIDBaseType">
							<xsd:annotation>
								<xsd:documentation>uID of a composite definition.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="orthotropyDirection" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Orthotropy direction of the composite.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="thicknessScaling" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Scaling factor of the composite thickness.
									Absolute thicknesses are defined in each composite material
									seperatly</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="materialUID" type="stringUIDBaseType">
							<xsd:annotation>
								<xsd:documentation>uID of a material definition.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="thickness" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Absolute thickness of the material.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="materialType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>materialType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Material type, conatining data of a material Material
							orientation: 1: orientation in first preferred direction 2:
							orientation in second preferred direction 3: remaining orthogonal
							direction other stiffness components are dependent on the given
							ones by symmetry relationships. 3 different material behaviours
							can be defined which is carried out by the 3 branches of the
							definition.</ddue:para>
						<ddue:list>
							<ddue:listItem>isotropic material like aluminium and steel may be
								defined</ddue:listItem>
							<ddue:listItem>transversal isotropic material like
								uni-directional layers of CFRP may be defined</ddue:listItem>
							<ddue:listItem>orthotrop material like several layers of CFRP may
								be defined</ddue:listItem>
						</ddue:list>
						<ddue:para>For each material behaviour the minimum number of
							stiffness parameters are requested. All other stiffness
							components are dependent by material symmetry relationships.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of material</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>description of material</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="rho" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Density of material in [kg/m^3]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="k11" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>stiffness component in 11-direction in [N/m]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="k12" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>stiffness component in 12-direction in [N/m]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:annotation>
							<xsd:documentation>choice between 3 different material behaviour
							</xsd:documentation>
						</xsd:annotation>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>mechanical properties for isotropic material
									other stiffness components are dependent on the given ones by
									symmetry relationships</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="sig11" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 11 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau12" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 12 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="sig11yieldT" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile yield strength in 11 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="sig11yieldC" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive yield strength in 11 direction
										in [N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>mechanical properties for transverse
									isotropic material Material orientation: 1: orientation in
									fiber direction 2 and 3: orientation orthogonal to fiber
									direction other stiffness components are dependent on the given
									ones by symmetry relationships</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="k22" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 22-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k23" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 23-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k66" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 66-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig11t" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 11 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig11c" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive strength in 11 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig22t" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 22 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig22c" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive strength in 22 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau12" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 12 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau23" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 23 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>mechanical properties for orthotropic
									material Material orientation: 1: orientation in first
									preferred direction 2: orientation in second preferred
									direction 3: remaining orthogonal direction</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="k13" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 13-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k22" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 22-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k23" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 23-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k33" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 33-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k44" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 44-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k55" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 55-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="k66" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>stiffness component in 66-direction
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig11t" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 11 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig11c" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive strength in 11 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig22t" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 22 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig22c" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive strength in 22 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig33t" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>tensile strength in 33 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sig33c" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>compressive strength in 33 direction in
										[N/m^2]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau12" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 12 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau13" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 13 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tau23" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>shear strength in 23 direction in [N/m^2]
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:choice>
					<xsd:element minOccurs="0" name="maxStrain" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum strain of the material. This value is
								intended for layerd material. Thus no orientation is given.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="fatigueFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>optional knockdown factor for fatiuqe
								(defaults to 1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="postFailure" type="postFailureType"/>
					<xsd:element minOccurs="0" name="thermalConductivity" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>
								thermal conductivity of the material in [W/(m*K)]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="specificHeatMap" type="specificHeatMapType"/>
					<xsd:element minOccurs="0" name="emissivityMap" type="emissivityMapType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="materialsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>materialsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Materials type, containing material and composite data.
							A material describes the properties of a certain material.
							Several materials can be combined within one composite.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="material" type="materialType"/>
					<xsd:element minOccurs="0" name="composites" type="compositesType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="microphoneType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>microphoneType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Microphone type, containing data of a microphone
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="point" type="pointType">
						<xsd:annotation>
							<xsd:documentation>Loaction of the microphone (x,y,z mandatory)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="groundReflectionCoeficient" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Coefficient for ground reflection effects
								(Sigma)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="misSimToolType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>misSimToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>MisSimTool type, containing MisSim tool data
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the aircraft model</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftMissionUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the aircraft's mission
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="timeStep" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Timestep of the flight path generation
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionDefinitionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionDefinitionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Containing mission and point performance definitions for performance evaluation of aircraft</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="segments" type="missionSegmentsType"/>
					<xsd:element minOccurs="0" name="missions" type="missionsType"/>
					<xsd:element minOccurs="0" name="pointPerformances" type="pointPerformancesType"/>
					<xsd:element minOccurs="0" name="segmentBlocks" type="missionSegmentBlocksType">
						<xsd:annotation>
							<xsd:documentation>optional entry for defining conditions for a predefined combination of segments (e.g.: setting the total range for a block of segments consisting of a takeOff, climb, cruise, descent and landing segment)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentEndConditionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentEndConditionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Conditions which signal the end of a mission segment</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
					<xsd:sequence>
						<xsd:element minOccurs="0" name="CAS" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Calibrated airspeed at the end of the segment in [m/s]</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="machNumber" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Mach number at the end of the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice minOccurs="0">
							<xsd:element name="positionXYZ" type="pointConstraintType">
								<xsd:annotation>
									<xsd:documentation>Global coordinate at the end of the segment in xyz coordinates</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="positionGeo" type="geographicPointConstraintType">
								<xsd:annotation>
									<xsd:documentation>Global coordinate at the end of the segment in geographic coordinates (longitude, latitude, altitude)</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
						<xsd:element minOccurs="0" name="range" type="pointConstraintType">
							<xsd:annotation>
								<xsd:documentation>Flown distance in the segment (x, y, z)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="duration" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Duration of the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="specificExcessPower" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Specific excess power at the end of the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="rateOfClimb" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Rate of climb ending the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="massFraction" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>MassFraction ending the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="fuelFraction" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Fuel mass fraction ending the segment, as remaining fuel mass with respect to initial segment fuel mass</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="fuelRemaining" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Remaining absolute fuel ending the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="fuelConsumed" type="doubleConstraintBaseType">
							<xsd:annotation>
								<xsd:documentation>Consumed fuel mass ending the segment</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="storesReleased" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>uID list of stores released in the segment (mostly comes joined with a new aeroPerformanceMapUID for the new aerodynamic configuration of the aircraft). Links to entries in the weightAndBalance vector such that new state is retrievable.
</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentBlockConstraintsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentBlockConstraintsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of constraints for the mission segment block</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="endCondition" type="missionSegmentEndConditionType"/>
							<xsd:element name="flightPath" type="flightPathType"/>
						</xsd:choice>
						<xsd:element minOccurs="0" name="variableSegments" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>Vector of segmentUIDs for segments that are allowed to be varied within a mission optimisation
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentBlockType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentBlockType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>segmentBlocks are optional definitions for grouping multiple segments and apply an endCondition to it (e.g.: start-climb-cruise-descend-land having a total range, or defining the outbound and inbound segments of a radiusOfAction type of mission)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
					<xsd:all>
						<xsd:element minOccurs="0" name="segmentBlockConstraints" type="missionSegmentBlockConstraintsType"/>
						<xsd:element minOccurs="0" name="segmentDirection">
							<xsd:annotation>
								<xsd:documentation>either 'outbound' or 'inbound', only needed for radiusOfAction kind of missions
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:restriction base="stringBaseType">
										<xsd:enumeration value="inbound"/>
										<xsd:enumeration value="outbound"/>
									</xsd:restriction>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="segmentUIDSequence" type="stringVectorBaseType"/>
						<xsd:element minOccurs="0" name="variableSegments" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>list of segments that are allowed to be varied within a mission optimisation.
									<ddue:para>This concept needs to be practically tested. Does it suffice to mention (a list of) segments that are free to change to fit the overall block constraints? What happens if a segment is variable, though it has some constraints? When to define a segment as variable (climb until endPosition z, then endPosition x should be left free. Is the segment then variable? Probably not.). Somehow the 'free' segment should be in between fully defined segments (i.e.: a cruise+descent in between endPosition z == ICA and endPosition z == 0 for landing to define max range. How to define this exactly?)
</ddue:para>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="fuelPlanningType">
							<xsd:annotation>
								<xsd:documentation>Entry for defining which type of mass the segment fuel mass should be added.
</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:restriction base="stringBaseType">
										<xsd:enumeration value="blockFuel"/>
										<xsd:enumeration value="taxiFuel"/>
										<xsd:enumeration value="tripFuel"/>
										<xsd:enumeration value="reserveFuel"/>
									</xsd:restriction>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
					</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentBlocksType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentBlocksType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>segmentBlocks are optional definitions for grouping multiple segments and apply an endCondition to it (e.g.: start-climb-cruise-descend-land having a total range, or defining the outbound and inbound segments of a radiusOfAction type of mission)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="segmentBlock" type="missionSegmentBlockType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of a mission segment which can be used to define missions</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
					<xsd:sequence>
						<xsd:element name="name" type="stringBaseType"/>
						<xsd:element minOccurs="0" name="description" type="stringBaseType"/>
						<xsd:element name="segmentType">
							<xsd:annotation>
								<xsd:documentation>Indicates the type of mission segment
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:restriction base="stringBaseType">
										<xsd:enumeration value="takeOff"/>
										<xsd:enumeration value="climb"/>
										<xsd:enumeration value="cruise"/>
										<xsd:enumeration value="descent"/>
										<xsd:enumeration value="landing"/>
										<xsd:enumeration value="acceleration"/>
									</xsd:restriction>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:choice>
							<xsd:element minOccurs="0" name="massFraction" type="massFractionType"/>
							<xsd:sequence>
								<xsd:element name="enginePerformanceMapUID" type="stringUIDBaseType">
									<xsd:annotation>
										<xsd:documentation>Refers to the enginePerformanceMap to be used within the segment
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="aeroPerformanceMapUID" type="stringUIDBaseType">
									<xsd:annotation>
										<xsd:documentation>Refers to the aeroPerformanceMap to be used within the segment
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element minOccurs="0" name="controlSurfaceUIDs" type="stringVectorBaseType">
									<xsd:annotation>
										<xsd:documentation>Indicates which control surface uID's to use for trimming purposes
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element minOccurs="0" name="endCondition" type="missionSegmentEndConditionType"/>
								<xsd:element minOccurs="0" name="constraintSettings" type="constraintSettingsType"/>
							</xsd:sequence>
						</xsd:choice>
					</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionSegmentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionSegmentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Containing a collection of mission segments which can be reused to define missions</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="missionSegment" type="missionSegmentType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionStartConditionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionStartConditionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Conditions which define the start of a mission</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:element name="runwayUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the runway at which the mission starts</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="CAS" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Calibrated airspeed at the start of the mission</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="mach" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Mach number at the start of the mission</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice minOccurs="0">
							<xsd:element name="positionXYZ" type="pointType">
								<xsd:annotation>
									<xsd:documentation>Global coordinate at the start of the mission in xyz coordinates</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="positionGeo" type="geographicPointType">
								<xsd:annotation>
									<xsd:documentation>Global coordinate at the start of the mission in geographic coordinates (longitude, latitude, altitude)</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
						<xsd:element minOccurs="0" name="orientation" type="orientationType">
							<xsd:annotation>
								<xsd:documentation>Orientation if the aircraft at the start of the mission</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Containing a collection of mission segments which can be reused to define missions</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
					<xsd:all>
						<xsd:element name="name" type="stringBaseType"/>
						<xsd:element minOccurs="0" name="description" type="stringBaseType"/>
						<xsd:element name="startCondition" type="missionStartConditionType"/>
						<xsd:element name="segmentUIDSequence" type="stringVectorBaseType"/>
					</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="missionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>missionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Containing a collection of missions</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="mission" type="missionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="modGenOptionalComponentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>modGenOptionalComponentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>ModGenOptional components type, containing optional
							components to be consideref in ModGen calculations</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="calculation" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Switch to include component into calculation
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="attachmentPoint">
						<xsd:annotation>
							<xsd:documentation>Attachment point to the structure
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="ALLCP"/>
									<xsd:enumeration value="FRONT"/>
									<xsd:enumeration value="REAR"/>
									<xsd:enumeration value="LOWER"/>
									<xsd:enumeration value="UPPER"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="attachmentRibs" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of ribs used for attaching the
								component</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="modGenOptionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>modGenOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>ModGenOptions type, containing ModGen options
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="divergenceMachCases" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mach numbers for divergence analysis cases. Up
								to 6 Ma Cases can be defined to analyze the divergence behavior
								for the according points on the flight envelope
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="optimizationMethod">
						<xsd:annotation>
							<xsd:documentation>Method for internal optimization: Modified
								Method of Feasible Directions [1] (default); Sequential Linear
								Programming [2]; Sequential Quadratic Programming [3]
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:minInclusive value="1"/>
									<xsd:maxInclusive value="3"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="landingGear" type="modGenOptionalComponentsType"/>
					<xsd:element minOccurs="0" name="engines" type="modGenOptionalComponentsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


