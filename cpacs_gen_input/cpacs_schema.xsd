	<xsd:complexType name="angleAccelerationType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>angleAccelerationType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:sequence>
						<xsd:element name="pDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around x (body fixed
									coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="qDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around y (body fixed
									coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="rDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around z (body fixed
									coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="phiDotDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around phi (earth fixed
									coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="thetaDotDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around theta (earth
									fixed coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="psiDotDot" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>Angular acceleration around psi (earth fixed
									coord.sys.)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="angleDisplacementType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>angleDisplacementType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Rotation order: psi, theta, phi</ddue:para>
					</ddue:remarks>
