	<xsd:complexType name="controlLawsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>controlLawsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Control Laws type, containing the aircraft's control
							laws</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
