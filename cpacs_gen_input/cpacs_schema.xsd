						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="optionalAux1" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>optional auxiliary parameter for special use
								(no physical meaning)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="optionalAux2" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>optional auxiliary parameter for special use
								(no physical meaning)</xsd:documentation>
