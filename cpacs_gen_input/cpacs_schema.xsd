						<xsd:annotation>
							<xsd:documentation>Reference to aircraft model
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="lax" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Einzelereignispegel LAX</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="origin">
						<xsd:annotation>
							<xsd:documentation>ICAO-Callsign of departure airport
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="ESKN"/>
									<xsd:enumeration value="EDFH"/>
									<xsd:enumeration value="EDDL"/>
									<xsd:enumeration value="EDDF"/>
									<xsd:enumeration value="EDDT"/>
									<xsd:enumeration value="ESSA"/>
									<xsd:enumeration value="EDVE"/>
									<xsd:enumeration value="EGSS"/>
									<xsd:enumeration value="EGLL"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="destination">
						<xsd:annotation>
							<xsd:documentation>ICAO-Callsign of arrival airport
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="ESKN"/>
									<xsd:enumeration value="EDFH"/>
									<xsd:enumeration value="EDDL"/>
									<xsd:enumeration value="EDDF"/>
									<xsd:enumeration value="EDDT"/>
									<xsd:enumeration value="ESSA"/>
									<xsd:enumeration value="EDVE"/>
									<xsd:enumeration value="EGSS"/>
									<xsd:enumeration value="EGLL"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="destinationRegion">
						<xsd:annotation>
							<xsd:documentation>Destination region of the flight
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="D"/>
									<xsd:enumeration value="UK"/>
									<xsd:enumeration value="EU"/>
									<xsd:enumeration value="EUR"/>
									<xsd:enumeration value="World"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="pax" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>ICAO-Callsign of departure airport
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="sTD" type="dateTimeBaseType">
						<xsd:annotation>
							<xsd:documentation>Scheduled time of departure
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="sTA" type="dateTimeBaseType">
						<xsd:annotation>
							<xsd:documentation>Scheduled time of arrival</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

