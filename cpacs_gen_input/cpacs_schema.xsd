					<xsd:element minOccurs="0" name="nOXFlow" type="stringVectorBaseType"/>
					<xsd:element minOccurs="0" name="sootFlow" type="stringVectorBaseType"/>
					<xsd:element minOccurs="0" name="hCFlow" type="stringVectorBaseType"/>
