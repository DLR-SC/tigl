					<xsd:element minOccurs="0" name="landing" type="genericCostType"/>
					<xsd:element minOccurs="0" name="ground" type="genericCostType"/>
					<xsd:element minOccurs="0" name="environmentalFees" type="genericCostType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
