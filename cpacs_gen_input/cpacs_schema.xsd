	<xsd:complexType name="transmissionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>transmissionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Transmissions type, containing all the
							transmissions/gearboxes of a rotorcraft model.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
