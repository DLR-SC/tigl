	<xsd:complexType name="observerDefinitionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>observerDefinitionType</ddue:para>
					</ddue:summary>
