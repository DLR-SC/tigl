	<xsd:complexType name="wingsCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>List of wing coefficient nodes for each wing
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains one subnode with coefficients for
										each of the wings.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;wing&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Aerodynamic coefficients for a single wing.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;calibration&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Aerodynamic coefficients for additional entities used for calibration.
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="wing" type="wingCoefficientsType"/>
					<xsd:element minOccurs="0" name="calibration" type="coefficientsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

