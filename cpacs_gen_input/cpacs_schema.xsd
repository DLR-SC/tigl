	<xsd:complexType name="maseratiInputsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>maseratiInputsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The maseratiInputsType contains parameters required by
