	<xsd:complexType name="bogieAxleType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of one axle attached to the bogie and its
							relative position on the bogie.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of one axle attached to the bogie and its
							relative position on the bogie.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="axleUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to the axle, that is attached to the
								bogie.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="posOnBogie" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative position of the axle on the bogie (if
								more than one axle defined). 0 = forward end of bogie; 1 = rear
								end of bogie.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
