	<xsd:complexType name="noseGearGlobalType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of global nose landing gear geometry
							parameters.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="totalLength" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Total length of nose landing gear, equals the
								distance form the middle of the bogie/axles to the axis of
								rotation of the pintle strut. Distance is measured while nose
								landing gear is fully extended.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="retractAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Retraction angle of the nose landing gear.
								Equals a rotation around the global z-axis in degrees. 0 =
								retraction to the front; 90 = retraction to the left; 180 =
								retraction to the rear; 270 = retraction to the right.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="extAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Angle between the full extended and the full
								retracted main strut.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="offset" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Distance from the upper end of the main strut
								to the axis of rotation of the pintle strut.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="extractedAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Angle of the main strut in extracted position
								around the landing gears y-axis. Defaults to 90 degrees which is
								vertal.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="noseGearType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of one nose landing gear.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>NoseGear type, containing all data of one single nose
							landing gear.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the nose landing gear.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the nose landing gear.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of part to which the gear is mounted (if
								any). For the majority of aircraft the parent of the nose
								landing gear is the fuselage. In each aircraft model, there is
								exactly one part without a parent part.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="fuselageAttachment" type="transformationType"/>
					<xsd:element minOccurs="0" name="global" type="noseGearGlobalType"/>
					<xsd:element minOccurs="0" name="components" type="gearComponentsType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
				<xsd:attribute name="symmetry">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="x-y-plane"/>
							<xsd:enumeration value="x-z-plane"/>
							<xsd:enumeration value="y-z-plane"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


