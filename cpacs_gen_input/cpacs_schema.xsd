					<xsd:element maxOccurs="unbounded" minOccurs="2" name="stringerPosition" type="stringerFramePositionType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
