	<xsd:complexType name="wheelsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of the landing gear wheels.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of the landing gear wheels.</ddue:para>
					</ddue:remarks>
