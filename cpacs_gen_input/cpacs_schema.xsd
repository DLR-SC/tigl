	<xsd:complexType name="bogieAxlesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Bogie axle configuration, i.e. list of axles attached
							to the bogie and their relative positions on the bogie.
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Bogie axle configuration, i.e. list of axles attached
							to the bogie and their relative positions on the bogie.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="axle" type="bogieAxleType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="bogieType">

		<xsd:annotation>
