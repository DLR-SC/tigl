	<xsd:complexType name="loadConditionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>flightLoadCaseType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>Main class</ddue:title>
								<ddue:content>
									<ddue:para>
										<ddue:legacyBold>State</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Includes general definition of aircraft attitude and
										the aircraft environment.</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Aircraft Settings</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Include definition of settings of the aircraft self.
										For example setting of a flap or landinggear.</ddue:para>
									<ddue:para>
										<ddue:legacyBold>MassCase</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Include definition about the massCase and is linked
										by an UID to the massBreakdown, weightAndBalance or direct
										component link. ?!</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>References</ddue:title>
								<ddue:content>
									<ddue:para>
										<ddue:legacyBold>Coordinate System(s)</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>Earth Fix coord.sys. (EF) with planar surface
												(x-y-Ebene = Earth surface, x North, y East)</ddue:listItem>
										</ddue:list>
										<ddue:para>
											<ddue:mediaLink>
												<ddue:image xlink:href="earthFixCoordSys"/>
											</ddue:mediaLink>
										</ddue:para>
										<ddue:list class="bullet">
											<ddue:listItem>refCoordUid (e.g. body fix and orientation to
												EF)</ddue:listItem>
											<ddue:listItem>refpointUid</ddue:listItem>
										</ddue:list>
										<ddue:para>
											<ddue:mediaLink>
												<ddue:image xlink:href="bodyFixCoordSys2"/>
											</ddue:mediaLink>
										</ddue:para>
									</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Euler angles</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>Attitude of body fix to earth fix coord.sys.
											</ddue:listItem>
											<ddue:listItem>ψ psi – heading</ddue:listItem>
											<ddue:listItem>θ theta – pitch angle</ddue:listItem>
											<ddue:listItem>Φ phi – bank angle</ddue:listItem>
											<ddue:listItem>Rotation order: ψ, θ, Φ</ddue:listItem>
										</ddue:list>
									</ddue:para>
									<ddue:para>
										<ddue:mediaLink>
											<ddue:image xlink:href="eulerAnglesInCoordSys"/>
										</ddue:mediaLink>
									</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Reference of attitude values
										</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>x,y,z (vector to ref point in EF)
											</ddue:listItem>
											<ddue:listItem>dx,dy,dz (vector in EF)</ddue:listItem>
											<ddue:listItem>u,v,w (vector in refCoord) (body fix)
											</ddue:listItem>
											<ddue:listItem>du,dv,dw (vector in refCoordUid) (body fix)
											</ddue:listItem>
										</ddue:list>
									</ddue:para>
									<ddue:para>
										<ddue:mediaLink>
											<ddue:image xlink:href="bodyFixCoordSys"/>
										</ddue:mediaLink>
									</ddue:para>
									<ddue:para>(dx,dy,dy --&gt; Euler transformation (psi, theta,
										phi) --&gt; u,v,w)</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="identifier">
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="Symmetric Trim Horizontal"/>
									<xsd:enumeration value="Symmetric Trim NZ"/>
									<xsd:enumeration value="Roll Trim Steady"/>
									<xsd:enumeration value="Roll Trim Accelerated"/>
									<xsd:enumeration value="Yaw Trim rudder deflect"/>
									<xsd:enumeration value="Yaw Trim beta overswing"/>
									<xsd:enumeration value="Yaw Trim beta steady"/>
									<xsd:enumeration value="Yaw Trim rudder neutral"/>
									<xsd:enumeration value="Discrete Gust"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="sizingType">
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="limit"/>
									<xsd:enumeration value="fatigue"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="safetyFactor" type="doubleBaseType"/>
					<xsd:element minOccurs="0" name="designSpeed">
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="VA"/>
									<xsd:enumeration value="VB"/>
									<xsd:enumeration value="VC"/>
									<xsd:enumeration value="VD"/>
									<xsd:enumeration value="VF"/>
									<xsd:enumeration value="Vs0"/>
									<xsd:enumeration value="Vs1"/>
									<xsd:enumeration value="VMC"/>
									<xsd:enumeration value="other"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="additionalParameters" type="additionalParametersType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="loadReferenceAxisPointType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
