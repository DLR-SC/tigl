	<xsd:complexType name="controlSurfaceHingeMomentMapsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>controlSurfaceHingeMomentMapsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>controlSurfaceHingeMomentMapsType type, containing the
							aerodynamic moment maps for one or more control surfaces.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="controlSurface" type="controlSurfaceHingeMomentMapType"/>
