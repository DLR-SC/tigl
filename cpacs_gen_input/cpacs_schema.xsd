	<xsd:complexType name="mapet2CCase1Type">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CCase1Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="doStabTrim" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Flag for stabilizer trim calculations before
								the take-off simulation runs. If turned off the stabilizer (or
								elevator) is not trimmed for positive climb at V2 and remains
								zero during the take-off run. 1 = Trim calculation ON 0 = Trim
								calculation OFF</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="estimatedV2" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Estimated V2 velocities for the stabilizer
								trim calculation. [m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="thetaV2" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Pitch angle required at safety speed V2 for
								stabilizer trim calculation. [deg]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="vIASRotInit" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Vector with different speeds VIAS in
								increas-ing order determining the speed for elevator deflection
								during the take-off run. The vec-tor defines the range of
								elevator deflection speeds and number of take-off simulations.
								[m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ctrl" type="mapet2CCtrlType">
						<xsd:annotation>
							<xsd:documentation>Main element containing the setting for the
								PID controllers imitating the pilot inputs during the take-off
								run.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="mapet2CCase2Type">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CCase2Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="vIASEngFail" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Vector with different speeds VIAS in
								increas-ing order determining the speed of one engine failure.
								The vector determines the speed range where the assumed VMCG is
								located. At least 4 different speeds are needed. [m/s]
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxLatDev" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum allowable lateral deviation from the
								runway centerline. [m]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ctrl" type="mapet2CCtrlType">
						<xsd:annotation>
							<xsd:documentation>Main element containing the setting for the
								PID rudder controller imitating the pilot in-puts during the
								take-off run.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="mapet2CCase3Type">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CCase3Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="maxTORunway" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum runway length. [m]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="vIASEngFail" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Vector with different speeds VIAS in
								increas-ing order determining the speed of one engine failure.
								The vector determines the speed range where the assumed V1 is
								locat-ed. [m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="vNGLimit" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum ground speed where nose gear steering
								is allowed. [m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxBrake" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum brake efficiency: 1 = 100percent brake
								efficiency 0 = 0percent brake efficiency</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ctrl" type="mapet2CCtrlType">
						<xsd:annotation>
							<xsd:documentation>Main element containing the setting for the
								PID controllers imitating the pilot inputs during the take-off
								run.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="mapet2CCase4Type">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CCase4Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="vIASLD" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Indicated air speed for aircraft landing.
								[m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="rwyCond" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Runway condition: 0 = Dry 1 = Wet 2 = Flooded
								3 = Icy This option is currently not supported by the 6-DOF
								model.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="useReverser" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Flag for using thrust reverser: 1 = Thrust
								reverser ON 0 = Thrust reverser OFF</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="useSpoiler" type="booleanBaseType">
						<xsd:annotation>
							<xsd:documentation>Flag for using ground spoiler: 1 = Ground
								spoiler ON 0 = Ground spoiler OFF</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="approachLength" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Length of the approach flight path over
								ground. [m]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="flareHeight" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Height above ground at aircraft landing flare.
								[m]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fpaTouchdown" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference flight path angle at touchdown
								point. [deg]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fpaStartApp" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference flight path angle at the begin-ning
								of the approach. [deg]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="rCgMldg" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Distance from aircraft CG to the main landing
								gear axis. [m]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="speedReverserOff" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Thrust reverser is turned off if the
								indicat-ed air speed is equal or lower than the value defined by
								this element [m/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxReverser" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum reverse thrust efficiency: 0 =
								0percent Reverse thrust 1 = 100percent Reverse thrust
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="maxBrake" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Maximum brake efficiency: 1 = 100percent brake
								efficiency 0 = 0percent brake efficiency</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ctrl" type="mapet2CCtrlType">
						<xsd:annotation>
							<xsd:documentation>Main element containing the setting for the
								PID controllers imitating the pilot in-puts during the landing
								approach.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="mapet2CCtrlType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CCtrlType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="pElev" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Proportional action coefficient for the flight
								path angle controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="pNg" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Proportional action coefficient for nose gear
								steering controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="pQ" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Proportional action coefficient for the pitch
								rate controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="pRud" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Proportional action coefficient for rudder
								controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="pThrust" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Proportional action coefficient for the
								ve-locity hold controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="iElev" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Integral action coefficient for flight path
								angle controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="iNg" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Integral action coefficient for nose gear
								steering controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="iQ" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Integral action coefficient for the pitch rate
								controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="iRud" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Integral action coefficient for rudder
								con-troller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="iThrust" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Integral action coefficient for the velocity
								hold controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dElev" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Derivative action coefficient for the flight
								path angle controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dNg" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Derivative action coefficient for nose gear
								steering controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dRud" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Derivative action coefficient for rudder
								con-troller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dThrust" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Derivative action coefficient for the
								veloci-ty hold controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dQ" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Derivative action coefficient for the pitch
								rate controller.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="theta1" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Initial reference for pitch angle after
								rota-tion. [deg]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="theta2" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Pitch angle reference 5 seconds after main
								landing gear liftoff. [deg]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="engRateLimit" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Rate limit for the engine thrust command.
								[1/s]</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="mapet2CToolParametersType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CToolParametersType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="analysisCase" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Specification of the performance analysis
								case: 1 = Rotation speed VR, liftoff speed VLOF and safety speed
								V2; 2 = Minimum control speed ground VMCG; 3 = Take-off decision
								speed V1; 4 = Landing distances</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftConfiguration" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Integer number defining the aircraft
								configuration used in the performance analysis.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="case1" type="mapet2CCase1Type">
						<xsd:annotation>
							<xsd:documentation>Main element with specific settings for the
								determination of rotation speed VR, liftoff speed VLOF and
								safety speed V2.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="case2" type="mapet2CCase2Type">
						<xsd:annotation>
							<xsd:documentation>Main element with specific settings for the
								determination of minimum control speed ground VMCG
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="case3" type="mapet2CCase3Type">
						<xsd:annotation>
							<xsd:documentation>Main element with specific settings for the
								determination of the take-off decision speed V1.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="case4" type="mapet2CCase4Type">
						<xsd:annotation>
							<xsd:documentation>Main element with specific settings for the
								determination of landing distances.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="mapet2CToolType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mapet2CToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Main element in the CPACS tool specific part
								for the MAPET2C performance calculation.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Aircraft model UID referring to the 6-DOF
								model generated with the flightSim tool.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftFlightCaseUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Flight case UID referring to the 6-DOF model
								generated with the flightSim tool.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="toolParameters" type="mapet2CToolParametersType">
						<xsd:annotation>
							<xsd:documentation>Main element containing the control parameters
								for MAPET2C execution.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="maseratiInputType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>maseratiInputType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The maseratiInputType contains input data required by
							the tool maserati.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
