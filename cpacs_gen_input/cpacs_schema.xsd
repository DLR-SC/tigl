	<xsd:complexType name="delisFuselagesSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisFuselagesSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>List of fuselages to set specific model and
						sizing settings. If not specified, default values are used.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="fuselage" type="delisFuselageSettingsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisProfileSizingSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisProfileSizingSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>Settings to define the sizing settings of
						beam/profile-like structures. Stringers that are smeared into the
						skin are not considered</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element minOccurs="0" name="stiffeningType">
						<xsd:annotation>
							<xsd:documentation>Choice determining the crosssection of the
								profile. The names and corresponding geometry is analog to the
								CPACS “standardProfileType Element”. An illustration is given in
								the CPACS documenation</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="T"/>
									<xsd:enumeration value="Z"/>
									<xsd:enumeration value="I"/>
									<xsd:enumeration value="J"/>
									<xsd:enumeration value="C"/>
									<xsd:enumeration value="L"/>
									<xsd:enumeration value="HAT"/>
									<xsd:enumeration value="CAP"/>
									<xsd:enumeration value="TUBE"/>
									<xsd:enumeration value="BOX"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="designCriteria" type="delisDesignCriteriaType"/>
					<xsd:element minOccurs="0" name="skinMinimalThickness" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Minimal thickness of a sheet(total stacking
								thickness) Defaults to 0.001</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisSheetSizingSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisSheetSizingSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>Settings to define the sizing settings of
						sheet-like structures. Since stringers may be modeled as smeared
						into the sheet/shell, stiffeners are also considered.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element minOccurs="0" name="stiffeningType">
						<xsd:annotation>
							<xsd:documentation>Choice determining the stiffening of the
								sheet. The names and corresponding geometry is analog to the
								CPACS “standardProfileType Element”.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="T"/>
									<xsd:enumeration value="Z"/>
									<xsd:enumeration value="I"/>
									<xsd:enumeration value="J"/>
									<xsd:enumeration value="C"/>
									<xsd:enumeration value="L"/>
									<xsd:enumeration value="HAT"/>
									<xsd:enumeration value="unstiffened"/>
									<xsd:enumeration value="sandwich"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="designCriteria" type="delisDesignCriteriaType"/>
					<xsd:element minOccurs="0" name="skinMinimalThickness" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Minimal thickness of a sheet(total stacking
								thickness) Defaults to 0.001</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisSizingConvergenceType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisSizingConvergenceType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="thicknessThreshold" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Convergence threshold to the maximum thickness
								change. Defaults to 0.0005 in SBOT</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="maxIterations" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Defaults to 13</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="massThreshold" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Defaults to 0.005</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisToolspecificType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisToolspecificType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>
								Tool identification
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftModelUID"
						type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>
								UID of the aircraft model
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lumpMassBreakdownOutput"
						type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>
								Flag if the massbreakdown should be
								written with every single structural
								item or if the mass sum of each type of
								structural element of one component
								should be written. E.g. write all
								sparCells or write only the sum of the
								sparCells of the wing.

								Defaults to false
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="coupleFuselageBT"
						type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>
								Flag if coupling with the BT fuselage
								should be performed. In this case, DELiS
								creates only wings and the ansys
								components for coupling with Trafuma. If
								False, also a DELiS fuselage will be
								created, if given and requested and
								coupled with the DELiS wings.

								Defaults to false
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="useAeroCoefficientAndNodalInertialLoads" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation>
                                Flag if the loadcase AeroCoefficientAndNodalInertialLoadsLoadCase should be used before other loadcases
        
                                Defaults to False
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
					<xsd:element minOccurs="0" name="createdComponents"
						type="delisCreatedComponentsType">
						<xsd:annotation>
							<xsd:documentation>
								Optional list of ComponentIDs of the
								component(wing or fuselage) that should
								be modeled. If not set all components
								are modeled.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="wings"
						type="delisWingsSettingsType">
						<xsd:annotation>
							<xsd:documentation>
								List of wings to set specific model and
								sizing settings. If not specified,
								default values are used.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="fuselages"
						type="delisFuselagesSettingsType">
						<xsd:annotation>
							<xsd:documentation>
								List of fuselages to set specific model
								and sizing settings. If not specified,
								default values are used.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="sizingConvergence"
						type="delisSizingConvergenceType" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisWingModelSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisWingModelSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>Settings that affect the wing finite element
						model.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="numberOfElementsInEtaDirection"
                        type="integerBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Number of Elements in span(eta)
                                direction. This also defines the number
                                of real and imaginary ribs in delis.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="numberOfElementsInXsiDirection"
                        type="integerBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Number of Elements in chord(xsi)
                                direction. This also defines the number
                                of all types of spars
                                (le,te,real,imaginary) in delis.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="skinBaysInSizingRegionEta"
                        type="integerBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Number of bays in span(eta) direction
                                that defines the size of the
                                optimization region. Defaults to 1
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="skinBaysInSizingRegionXsi"
                        type="integerBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Number of bays in chord(xsi) direction
                                that defines the size of the
                                optimization region. Defaults to the
                                number of bays between adjacent spars
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="createOnlyWingBox"
                        type="booleanBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Flag if just the wing box(region from
                                front spar to rear spar) should be
                                modeled. Otherwise a model from leading
                                to trailing edge is created. Defaults to
                                True
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="createControlSurfaces"
                        type="booleanBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Flag if the control surfaces should be
                                modeled explicitly instead of using mass
                                points Defaults to False
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <!--
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="stringerModeling"
                        type="delisBeamModelingBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Determines how stringers are modeled.
                                These strings can be given: [smeared,
                                simpleBeamSection, complexBeamSection]
                                Defaults to smeared
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="capModeling"
                        type="delisBeamModelingBaseType">
                        <xsd:annotation>
                            <xsd:documentation>
                                Determines how caps are modeled. These
                                strings can be given:
                                [simpleBeamSection, complexBeamSection].
                                Defaults to simpleBeamSection.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    -->
                    <xsd:element name="createStructureTopology"
                        type="booleanBaseType" maxOccurs="1"
                        minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>
                                Flag if wing structure should be created
                                automatically via aircraft generator
                                class without an input dataset. If the
                                flag is set to false, the inner
                                structure needs to be read from CPACS or
                                any other input dataset.

                                Attention: At the moment, only ribs will
                                be created if True.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="splitWingStructure" type="booleanBaseType" maxOccurs="1" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Flag if wing structure(spars and skins) defined in cpacs should be split
								into smaller regions bordered by spars and ribs. If False, the initial cpacs definition of sparSegments
								and the default skin definition is used instead of a fine granular structure. This has effects on
								the sizing regions, which are coarse or fine.
								
								Defaults to True 
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisWingSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisWingSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>Wing model and sizing settings. If items are not
						specified, the default values are used.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="wingUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>ID of the wing</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="modelSettings" type="delisWingModelSettingsType">
						<xsd:annotation>
							<xsd:documentation>Settings that affect the wing’s finite element
								model. The splitting of the optimization regions still account
								for the given splitting in CPACS. So if some certain structure
								is split more fine, while these parameters are coarse, the fine
								split stays intact!</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="sizingSettings" type="delisWingSizingSettingsType">
						<xsd:annotation>
							<xsd:documentation>Each structure element listed can be modified
								in terms of sizing properties.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisWingSizingSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisWingSizingSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>Each structure element listed can be modified in
						terms of sizing properties.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element minOccurs="0" name="ribSizingSettings" type="delisSheetSizingSettingsType"/>
					<xsd:element minOccurs="0" name="frontSparSizingSettings" type="delisSheetSizingSettingsType">
						<xsd:annotation>
							<xsd:documentation>Sizing settings for the front spar.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="sparSizingSettings" type="delisSheetSizingSettingsType">
						<xsd:annotation>
							<xsd:documentation>Sizing settings for all spars except the front
								spar.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="upperSkinSizingSettings" type="delisSheetSizingSettingsType"/>
					<xsd:element minOccurs="0" name="lowerSkinSizingSettings" type="delisSheetSizingSettingsType"/>
					<xsd:element minOccurs="0" name="capSizingSettings" type="delisProfileSizingSettingsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="delisWingsSettingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>delisWingsSettingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:annotation>
					<xsd:documentation>List of wings to set specific model and sizing
						settings. If not specified, default values are used.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="wing" type="delisWingSettingsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="deltaTemperatureType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>deltaTemperatureType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="total" type="stringVectorBaseType"/>
					<xsd:element name="cO2" type="stringVectorBaseType"/>
					<xsd:element name="h2O" type="stringVectorBaseType"/>
					<xsd:element name="o3" type="stringVectorBaseType"/>
					<xsd:element name="cH4" type="stringVectorBaseType"/>
					<xsd:element name="cont" type="stringVectorBaseType"/>
					<xsd:element name="pmo" type="stringVectorBaseType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="designMassesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Design masses</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The design mases are requerments wich can com form the
							TLARs</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="mTOM" type="genericMassType">
						<xsd:annotation>
							<xsd:documentation>Take off mass</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="mZFM" type="genericMassType">
						<xsd:annotation>
							<xsd:documentation>Zero Fuel mass</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="mMLM" type="genericMassType">
						<xsd:annotation>
							<xsd:documentation>Maximum landing mass</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="mMRM" type="genericMassType">
						<xsd:annotation>
							<xsd:documentation>Maximum ramp mass (the maximum weight
								authorised for the ground handling)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="designParameterType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Design parameter definition</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Contains a the values of a parameter and its uid as reference.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="parameterUID" type="stringUIDBaseType"/>
					<xsd:element name="values" type="stringVectorBaseType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	<xsd:complexType name="designParametersType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Design parameters list</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Contains a list of all design parameters.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="designParameter" type="designParameterType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	<xsd:complexType name="designSetType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>designSetType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of a design set for use in fDesignRC
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="name" type="stringBaseType"/>
					<xsd:element name="fuselageXthickestFact" type="doubleBaseType"/>
