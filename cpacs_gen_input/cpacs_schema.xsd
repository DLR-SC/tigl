					<xsd:element maxOccurs="unbounded" minOccurs="0" name="bkLoadCaseUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Sequence of Loadcases to be considered in
								sizing loop
								(preliminary BK definition)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
