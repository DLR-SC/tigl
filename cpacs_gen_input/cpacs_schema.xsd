                <xsd:element minOccurs="0" name="sheetBasedStructuralElements" type="sheetBasedStructuralElementsType"/>
                <xsd:element minOccurs="0" name="profileBasedStructuralElements" type="profileBasedStructuralElementsType"/>
                <xsd:element minOccurs="0" name="pressureBulkheads" type="pressureBulkheadsType"/>
                <xsd:element minOccurs="0" name="windows" type="windowsType"/>
                <xsd:element minOccurs="0" name="doors" type="doorsType"/>
                <xsd:element minOccurs="0" name="doorSurroundStructures" type="doorSurroundStructuresAssemblyType"/>
                <xsd:element minOccurs="0" name="seatModules" type="seatModulesType"/>
                <xsd:element minOccurs="0" name="rivets" type="rivetsType"/>
            </xsd:all>
