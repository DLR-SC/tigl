	<xsd:complexType name="coefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Data type containing a set of aerodynamic coefficients
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type can contain a complete set of aerodynamic
										force- and moment coefficients. All coefficients are given in
										the CPACS-Coordinate system. See the documentation for the
										global CPACS-Element for further information.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;cfx&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										x-direction, normalized with the global reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;cfy&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										y-direction, normalized with the global reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;cfz&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										z-direction, normalized with the global reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;cmx&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										x-axis and the global reference point, normalized with the
										global reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;cmy&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										y-axis and the global reference point, normalized with the
										global reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;cmz&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										z-axis and the global reference point, normalized with the
										global reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="cfx" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-x direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfy" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-y direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfz" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-z direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmx" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-x direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmy" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-y direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmz" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-z direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


