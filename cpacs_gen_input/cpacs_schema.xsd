					<xsd:element minOccurs="0" name="payload" type="payloadGlobalType"/>
					<xsd:element minOccurs="0" name="airportCompatability" type="airportCompatabilityGlobalType">
					</xsd:element>
					<xsd:element minOccurs="0" name="performanceTargets" type="performanceTargetsGlobalType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="aircraftModelType">

		<xsd:annotation>
