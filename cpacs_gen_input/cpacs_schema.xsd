	<xsd:complexType name="aeroFuseFuselagePanelingType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>aeroFuseFuselagePanelingType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>AeroFuseFuselagePaneling type, containing the
							circumferential and streamwise paneling for the fuselages of the
							VSAero model</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="fuselageUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a fuselage; required, if separate
								paneling for each fuselage is used. If missing, paneling is used
								for all fuselages of the selected model which have no explicit
								fuselagePaneling.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="streamwise" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Streamwise paneling for each fuselage
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="circumferential" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Circumferential paneling for each fuselage
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

