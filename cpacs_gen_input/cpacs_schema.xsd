				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="externalLoad" type="bkexternalLoadType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="blockedDOFType">

