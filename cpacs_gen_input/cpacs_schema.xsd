						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="singleMicrophone"/>
									<xsd:enumeration value="loadArray"/>
									<xsd:enumeration value="createArray"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:choice>
						<xsd:element name="filename" type="stringBaseType">
							<xsd:annotation>
								<xsd:documentation>Name of the observer array file (on the
									analysis server)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence>
							<xsd:element name="symmetry" type="booleanBaseType">
								<xsd:annotation>
									<xsd:documentation>Switch for symmetry condition
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="distanceGroundTrack" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Maximum observer distance to flight ground
										track [m]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="arrayDelta" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Microphone distance [m]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="micHeight" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Microphone height [m]</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element maxOccurs="39" name="microphone" type="microphoneType"/>
						</xsd:sequence>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
