	<xsd:complexType name="fuselageSectionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageSectionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>FuselageSection type, containing fusleage section and
							element data</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of fuselage section</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of fuselage section
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transformation" type="transformationType"/>
					<xsd:element name="elements" type="fuselageElementsType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselageSectionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageSectionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>FuselageSections type, containing fuselage sections
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="2" name="section" type="fuselageSectionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselageSegmentCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>The aerodynamic coefficients of a fuselage segment
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains the aerodynamic coefficients of
										one fuselage segment.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;segmentUID&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A reference to the fuselage segment.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;strip&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A list of nodes, each containing the local
										aerodynamic coefficients of a fuselage segment's strip.
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="segmentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to the fuselage segment
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" name="strip" type="fuselageSegmentStripCoefficientsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="fuselageSegmentStripCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Aerodynamic force- and moment coefficients of a fuselage
							strip</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type can contain a complete set of aerodynamic
										force- and moment coefficients for a fuselage strip. It also
										includes the local reference values belonging to the strip
										coefficients. All coefficients act in the CPACS coordinate
										system. See the documentation for the global CPACS-Element for
										further information.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;cfx&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										x-direction, normalized with the local reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;cfy&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										y-direction, normalized with the local reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;cfz&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Force coefficient in (global, i.e. CPACS)
										z-direction, normalized with the local reference area.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;cmx&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										x-axis and the local reference point, normalized with the
										local reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;cmy&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										y-axis and the local reference point, normalized with the
										local reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;cmz&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Moment coefficient around the (global, i.e. CPACS)
										z-axis and the local reference point, normalized with the
										local reference area and reference length.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>7.
									&lt;reference&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Reference values for the strip's coefficients.
										Moment reference point is given in absolut CPACS coordinates,
										i.e. x,y,z.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="cfx" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-x direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfy" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-y direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfz" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Force coefficient in CPACS-z direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmx" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-x direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmy" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-y direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmz" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Moment coefficient in CPACS-z direction
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="reference" type="loadReferenceType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>	


	<xsd:complexType name="fuselageSegmentType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageSegmentType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>FuselageSegment type, containing data of a fuselage
							segment</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of fuselage segment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of fuselage segment
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fromElementUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to element from which the segment
								shall start</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="toElementUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to element at which the segment
								shall end</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="guideCurves" type="guideCurvesType">
						<xsd:annotation>
							<xsd:documentation>Optional and additional guidecurves to shape
								the outer geometry.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselageSegmentsCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>A list of fuselage segments with their coefficients.
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains one subnode with aerodynamic
										coefficients for each of the fuselage's segments.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;segment&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A node with the coefficient distributions on one
										fuselage.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="segment" type="fuselageSegmentCoefficientsType"/>
					<xsd:element minOccurs="0" name="calibration" type="coefficientsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="fuselageSegmentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageSegmentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>FuselageSegments type, containing fuselage segment
							definitions (from sections and elements)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="segment" type="fuselageSegmentType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


<xsd:complexType name="fuselageStructureType">
    <xsd:annotation>
        <xsd:appinfo>
            <sd:schemaDoc>
                <ddue:summary>
                    <ddue:para>fuselageStructureType</ddue:para>
                </ddue:summary>
                <ddue:remarks>
                    <ddue:para>FuselageStructure type, containing data of the fuselage's
                        structure</ddue:para>
                </ddue:remarks>
            </sd:schemaDoc>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
        <xsd:extension base="complexBaseType">
            <xsd:all>
                <xsd:element minOccurs="0" name="skin" type="skinType"/>
                <xsd:element minOccurs="0" name="stringers" type="stringersAssemblyType"/>
                <xsd:element minOccurs="0" name="frames" type="framesAssemblyType"/>
                <xsd:element minOccurs="0" name="paxCrossBeams" type="paxCrossBeamsAssemblyType"/>
                <xsd:element minOccurs="0" name="paxCrossBeamStruts" type="paxCrossBeamStrutsAssemblyType"/>
                <xsd:element minOccurs="0" name="cargoCrossBeams" type="cargoCrossBeamsAssemblyType"/>
                <xsd:element minOccurs="0" name="cargoCrossBeamStruts" type="cargoCrossBeamStrutsAssemblyType"/>
                <xsd:element minOccurs="0" name="longFloorBeams" type="longFloorBeamsAssemblyType"/>
                <xsd:element minOccurs="0" name="generalStructuralMembers" type="generalStructuralMembersAssemblyType"/>
                <xsd:element minOccurs="0" name="floorPanels" type="floorPanelsType"/>
                <xsd:element minOccurs="0" name="pressureBulkheads" type="pressureBulkheadAssemblyType"/>
                <xsd:element minOccurs="0" name="windows" type="windowsAssemblyType"/>
                <xsd:element minOccurs="0" name="paxDoors" type="paxDoorsAssemblyType"/>
                <xsd:element minOccurs="0" name="cargoDoors" type="cargoDoorsAssemblyType"/>
                <xsd:element minOccurs="0" name="intercostals" type="intercostalsAssemblyType"/>
                <xsd:element minOccurs="0" name="centerFuselageAreas" type="centerFuselageAreasAssemblyType"/>
                <xsd:element minOccurs="0" name="tailplaneAttachmentArea" type="tailplaneAttachmentAreaType"/>
                <xsd:element minOccurs="0" name="interfaceDefinitions" type="interfaceDefinitionsType"/>
                <xsd:element minOccurs="0" name="UIDGroupDefinitions" type="UIDGroupDefinitionsType"/>
            </xsd:all>
        </xsd:extension>
    </xsd:complexContent>
</xsd:complexType>



	<xsd:complexType name="fuselageType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>Fuselage type, containing all data related to a
								fuselage</ddue:para>
							<ddue:mediaLink>
								<ddue:image xlink:href="geomBlock"/>
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>
								Name of fuselage
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>
								Description of fuselage
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>
								UID of part to which the fuselage is
								mounted (if any)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transformation" type="transformationType"/>
					<xsd:element name="sections" type="fuselageSectionsType"/>
					<xsd:element minOccurs="0" name="positionings" type="positioningsType"/>
					<xsd:element name="segments" type="fuselageSegmentsType"/>
					<xsd:element minOccurs="0" name="structure" type="fuselageStructureType"/>
					<xsd:element minOccurs="0" name="decks" type="decksType"/>
					<xsd:element minOccurs="0" name="dynamicAircraftModel" type="dynamicAircraftModelType"/>
					<xsd:element minOccurs="0" name="fuelTanks" type="fuselageFuelTanksType"/>
					<xsd:element minOccurs="0" name="cutOuts" type="fuselageCutOutsType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
				<xsd:attribute name="symmetry">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="x-y-plane"/>
							<xsd:enumeration value="x-z-plane"/>
							<xsd:enumeration value="y-z-plane"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselagesAeroPerformanceType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselagesAeroPerformanceType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>fuselagesAeroPerformance type, containing
							fuselagesAeroPerformance</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="fuselageAeroPerformance" type="fuselageAeroPerformanceType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselagesCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>List of fuselage coefficient nodes for each wing
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains one subnode with coefficients for
										each of the fuselages.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;fuselage&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Aerodynamic coefficients for a single fuselage.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;calibration&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Aerodynamic coefficients for additional entities used for calibration.
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="fuselage" type="fuselageCoefficientsType"/>
					<xsd:element minOccurs="0" name="calibration" type="coefficientsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="fuselagesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselagesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Fuselages type, containing the fuselages of the
							configuration</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="fuselage" type="fuselageType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="gearComponentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Detailed description of all components of the
							landing gear.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>Detailed description of the following components of
								the landing gear: mainStrut, piston, dragStrut, pintleStrut,
								sideStrut, mainActuator, bogie, axle and wheels.</ddue:para>
							<ddue:para>The definition of the components include geometrical
								as well as material properties.</ddue:para>
							<ddue:para>The definition of the different components can be
								found in the pictures below for the main and the nose landing
								gear:</ddue:para>
							<ddue:mediaLink>
								<ddue:image xlink:href="mlg"/>
							</ddue:mediaLink>
							<ddue:mediaLink>
								<ddue:image xlink:href="nlg"/>
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="mainStrut" type="strutType"/>
					<xsd:element name="piston" type="pistonType"/>
					<xsd:element name="dragStrut" type="strutWithActuatorType"/>
					<xsd:element name="pintleStrut" type="strutType"/>
					<xsd:element name="sideStruts" type="sideStrutsType"/>
					<xsd:element name="mainActuator" type="mainActuatorType"/>
					<xsd:element minOccurs="0" name="bogie" type="bogieType"/>
					<xsd:element name="axles" type="axlesType"/>
					<xsd:element name="wheels" type="wheelsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="gearDeflectionVectorType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>This type contains a vector of deflection values for a
							single gear</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>In this type, a vector of deflections of a single
										gear is specified.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;gearUID&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A reference to a gear from the aircraft model
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;relDeflection&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A vector of relative deflections of the selected
										gear</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="gearUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a gear</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="relDeflection" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Relative deflections of the gear
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="gearDeflectionVectorsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>This type contains a list of gears and their deflection
							vectors</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>In this type, a list of gears is defined.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;gear&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>One of these nodes per deflected gear is required
										here.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="gear" type="gearDeflectionVectorType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="generalStructuralMemberPositionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>stringerFramePositionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>stringerFramePosition type, containing individual
							stringer / frame postion definition (CPACS V2.1+)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="structuralElementUID" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionX" type="doubleBaseType"/>
					<xsd:element minOccurs="0" name="positionXUidStart" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionXUidEnd" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionXUidMin" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionXUidMid" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionXUidMax" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="positionXAngle" type="doubleBaseType"/>
					<xsd:element minOccurs="0" name="positionXFact" type="doubleBaseType"/>
					<xsd:element minOccurs="0" name="positionXoffset" type="doubleBaseType"/>
					<xsd:element minOccurs="0" name="alignment" type="alignmentStringFrameType"/>
					<xsd:element default="0" minOccurs="0" name="continuity">
						<xsd:annotation>
							<xsd:documentation>Continuity definition for profile extrusion:
								0= C0 (allows sharp edges, default), 2= C2 (defines curvature
								continuity)</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="2"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element default="0" minOccurs="0" name="interpolation">
						<xsd:annotation>
							<xsd:documentation>Definition of interpolation between different
								profiles: 0= no interpolation 1= interpolation of strcutural
								profile</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="generalStructuralMemberType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>generalStructuralMemberType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="standardMemberPosition" type="generalStructuralMemberPositionType"/>
					<xsd:element maxOccurs="unbounded" minOccurs="2" name="structuralMemberPosition" type="generalStructuralMemberPositionType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="generalStructuralMembersAssemblyType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>generalStructuralMembersAssemblyType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>generalStructuralMembersAssembly type, containing
							structural member assemblys</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="generalStructuralMember" type="generalStructuralMemberType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericComponentCoefficientsType">
		<xsd:annotation><xsd:appinfo><sd:schemaDoc><ddue:summary><ddue:para>genericComponentCoefficientsType</ddue:para></ddue:summary><ddue:remarks><ddue:para/></ddue:remarks></sd:schemaDoc></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="complexBaseType"><xsd:sequence>
			<xsd:element name="parentUID" type="stringUIDBaseType"/>
			<xsd:element name="coefficients" type="coefficientsType"/>
		</xsd:sequence>
	</xsd:extension></xsd:complexContent></xsd:complexType>

	<xsd:complexType name="genericComponentsCoefficientsType">
		<xsd:annotation><xsd:appinfo><sd:schemaDoc><ddue:summary><ddue:para>genericComponentsCoefficientsType</ddue:para></ddue:summary><ddue:remarks><ddue:para/></ddue:remarks></sd:schemaDoc></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="complexBaseType"><xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="genericComponent" type="genericComponentCoefficientsType"/>
		</xsd:sequence>
	</xsd:extension></xsd:complexContent></xsd:complexType>

	<xsd:complexType name="genericCostType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>genericCostType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="name" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="description" type="stringBaseType"/>
					<xsd:element name="cost" type="doubleBaseType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericGeometricComponentType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>genericGeometricComponentType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>In some cases additional geometric components need to
								be linked to a CPACS, but these components are not yet handled by
								CPACS explicitly. For example, a belly fairing and/or external
								tanks.</ddue:para>
							<ddue:para>A generic geometric component may be applied to include
								such a geometry from an external file (preferably STEP) in the
								context of the overall aircraft.</ddue:para>
							<ddue:mediaLink>
								<ddue:image xlink:href="geomBlock"/>
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of genericGeometricComponent
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of genericGeometricComponent
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of part to which the component is mounted
								(if any)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transformation" type="transformationType"/>
					<xsd:element name="linkToFile" type="linkToFileType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
				<xsd:attribute name="symmetry">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="x-y-plane"/>
							<xsd:enumeration value="x-z-plane"/>
							<xsd:enumeration value="y-z-plane"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericGeometryComponentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>genericGeometryComponentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="genericGeometryComponent" type="genericGeometricComponentType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericMassType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>mass</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>genericMassType, describing mass, inertia and CoG data.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>description</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>parentUID</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="mass" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>mass value</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="location" type="pointType">
						<xsd:annotation>
							<xsd:documentation>mass loaction values (x,y,z)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="orientation" type="pointType">
						<xsd:annotation>
							<xsd:documentation>mass orientation values</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="massInertia" type="massInertiaType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericSystemsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>genericSystemsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Node for geometrical layout of system components
							based on simple geometric shapes</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="genericSystem" type="genericSystemType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="genericSystemType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Generic system type containing describing the basic dimensions of a system component using simple geometric shapes</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>The generic base types must be one of the following cone|sphere|cube|cylinder. The origin of the shapes is at the center of volume for cube, sphere and cylinder. Since the cone is constructued from a cylindric shape, its origin is at the same location as the origin of the basic cylinder, although not any longer the center of volume due to scaling of one side. While cube and sphere are symmetrical in all three axis, cylinder and cone are standing upright in the direction of cpacs z-axis. The tip of the cone is located in direction of the positive z-axis. The figure below illustrates the default position and orientation of the different shapes. Note that the cube, cylinder and sphere were moved along the x-axis for demonstration by 2, 4 and 6 meters respectively.</ddue:para>
							<ddue:para>For spheres the default diameter is 1m. Cubes have a length of 1m in all three dimensions. Cylinders have a length of 1m and a diameter of 1m for the circular cross section. The cone is a cylinder with the top face scaled down to zero.
							</ddue:para>
							<ddue:mediaLink>
								<ddue:image xlink:href="geometricBaseTypes"/>
							</ddue:mediaLink>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the system component.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the system component.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="geometricBaseType">
						<xsd:annotation>
							<xsd:documentation>Enum for selecting the basic shape of the 
								component</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="cone"/>
								<xsd:enumeration value="sphere"/>
								<xsd:enumeration value="cube"/>
								<xsd:enumeration value="cylinder"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="transformation" type="transformationType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
				<xsd:attribute name="symmetry">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="x-y-plane"/>
							<xsd:enumeration value="x-z-plane"/>
							<xsd:enumeration value="y-z-plane"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenAssemblyType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenAssemblyType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenAssemblyType contains all informations
							required by GeoGen to assemble fuselages, wings and rotors in
							CATIA and to output the resulting geometries into the CPACS
							dataset.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="reference" type="geogenReferenceType">
						<xsd:annotation>
							<xsd:documentation>Global reference lengths and point used by
								GeoGen</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="constraints" type="geogenConstraintsType">
						<xsd:annotation>
							<xsd:documentation>Geometry constraints to be checked
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="fuselages" type="geogenFuselagesType">
						<xsd:annotation>
							<xsd:documentation>Definition of fuselage assemblies
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="rotorBlades" type="geogenRotorBladesType">
						<xsd:annotation>
							<xsd:documentation>Definition of rotorBlade assemblies
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="wings" type="geogenWingsType">
						<xsd:annotation>
							<xsd:documentation>Definition of wing assemblies
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenAttachedMassPositionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenAttachedMassPositionType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenAttachedMassPositionType is used to define a
							component mass position using a CATIA UDF from the GeoGen
							Catalog, which outputs a point in the CATIA modrl built by
							GeoGen. GeoGen will output the global coordinates of each
							geogenAttachedMassPosition to the XPath
							(locationGlobalDestinationXPath) given by the user.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the attached mass.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the attached mass.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="catalogPointComponentUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the attachment component, as defined in
								the component catalog.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parameters" type="geogenParametersType">
						<xsd:annotation>
							<xsd:documentation>Parameter definitions for the attachment
								component. The possible parameter definitions depend on the
								component definition in the component catalog.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="componentReferences" type="geogenComponentReferencesType">
						<xsd:annotation>
							<xsd:documentation>References to components which were created by
								geogen previously.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="destinationXPath" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>XPath of the massDescription to which the
								calculated global coordinates should be written. If omitted, the
								mass position will not be written to the output CPACS file.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Unique ID of the attachmentMassPosition.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenAttachedMassPositionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenAttachedMassPositionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenAttachedMassPositionsType contains a
							collection of GeoGen attachedMassPosition definitions.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="attachedMassPosition" type="geogenAttachedMassPositionType">
						<xsd:annotation>
							<xsd:documentation>GeoGen attachedMassPositions definition
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenComponentReferenceType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenComponentReferenceType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenComponentReferenceType can be used to refer
							to an existing component instead of creating a new component.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="catalogSubcomponentUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the subcomponent as defined in the
								component catalog</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="refUID" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>UID of the existing component to which the
							reference should point. This component must be a child element of
							the same</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenComponentReferencesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenComponentReferencesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenComponentReferencesType contains a collection
							of references to existing components</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="componentReference" type="geogenComponentReferenceType">
						<xsd:annotation>
							<xsd:documentation>Include one &lt;componentReference&gt; element
								for each component reference definition</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenComponentType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenComponentType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenComponentType contains all information needed
							by GeoGen to create an instance of a component from the component
							catalog</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the component</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Optional description of the component
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="catalogSubcomponentUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the subcomponent as defined in the
								component catalog. Required if the component is a subcomponent
								of another component.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="catalogComponentUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the component as defined in the
								component catalog</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parameters" type="geogenParametersType">
						<xsd:annotation>
							<xsd:documentation>Parameter definitions for the component. The
								possible parameter definitions depend on the component
								definition in the component catalog.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="components" type="geogenComponentsType">
						<xsd:annotation>
							<xsd:documentation>Subcomponent definitions for the component.
								The possible subcomponent definitions depend on the component
								definition in the component catalog.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="componentReferences" type="geogenComponentReferencesType">
						<xsd:annotation>
							<xsd:documentation>References to components which where defined
								elsewhere but should be used as subcomponents
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Unique ID of the component. Can be used to
							refer to this component using a &lt;componentReference&gt;
							element</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenComponentsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenComponentsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenComponentsType contains a collection of
							(sub-)components</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="component" type="geogenComponentType">
						<xsd:annotation>
							<xsd:documentation>Include one &lt;component&gt; element for each
								(sub-)component definition</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenConstraintOffsetType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenConstraintOffsetType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenConstraintOffsetType defines the offset
							distance for offset surfaces used as constraints for GeoGen.
						</ddue:para>
						<ddue:para>For inner boundaries the offset will be added to the
							original surface, while for outer boundaries the offset will be
							subtracted from the original surface.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="stringBaseType">
				<xsd:attribute name="mode" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Mode for calculation of the offset surface: 1 =
							Normal to the surface, 2 = z-Direction</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenConstraintType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenConstraintType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenConstraintType can be used to define inner or
							outer boundary surfaces which shall not be penetrated by the
							resulting surface geometries.</ddue:para>
						<ddue:para>Constraints behave differently for inner and outer
							boundaries:</ddue:para>
						<ddue:list class="bullet">
							<ddue:listItem>
								<ddue:para>
									<ddue:legacyBold>Inner boundaries:</ddue:legacyBold>
									The volume enclosed by the referenced fuselage surface and
									thickened by the optional offset has to be within the volume
									enclosed by the created fuselages and must not penetrate it's
									boundaries.
								</ddue:para>
							</ddue:listItem>
							<ddue:listItem>
								<ddue:para>
									<ddue:legacyBold>Outer boundaries:</ddue:legacyBold>
									The volume enclosed by the referenced fuselage surface minus
									the optional offset must contain the volume enclosed by the
									created fuselages. The created fuselage surface must not
									penetrate it's boundaries.
								</ddue:para>
							</ddue:listItem>
						</ddue:list>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="fuselageUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the fuselage which should be used as
								boundary. This fuselage definition must exist in the CPACS
								dataset before GeoGen is run and it must be a valid geometry.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="offset" type="geogenConstraintOffsetType">
						<xsd:annotation>
							<xsd:documentation>Optional offset which is added to (for inner
								boundaries) or subtracted from (for outer boundaries) the
								referenced fuselage's surface to create the final boundary
								surface.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenConstraintsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenConstraintsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of the inner and outer boundaries which
							should not be penetrated by the resulting geometries.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="innerBounds" type="geogenConstraintType">
						<xsd:annotation>
							<xsd:documentation>Inner boundary for the resulting geometries
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outerBounds" type="geogenConstraintType">
						<xsd:annotation>
							<xsd:documentation>Outer boundary for the resulting geometries
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenFuselageOutputOptionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenFuselageOutputOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenFuselageOutputOptionsType contains options
							used during the export of a fuselage from CATIA to CPACS.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="destinationFuselageUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Destination UID of the fuselage which will be
								written into the CPACS file.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="sectionDistributionMode">
						<xsd:annotation>
							<xsd:documentation>Section distribution mode. If not specified,
								the value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="automatic"/>
									<xsd:enumeration value="distributionLawEquidistant"/>
									<xsd:enumeration value="distributionLawFullCosine"/>
									<xsd:enumeration value="distributionLawHalfCosineFront"/>
									<xsd:enumeration value="distributionLawHalfCosineRear"/>
									<xsd:enumeration value="manual"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of sections. If not specified, the
								value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of points per section. If not
								specified, the value specified in the global output options is
								used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenFuselageType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenFuselageType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenFuselageType contains all information needed
							by GeoGen to assemble component templates as a fuselage and to
							export the geometry to CPACS.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="reference" type="geogenReferenceType">
						<xsd:annotation>
							<xsd:documentation>Reference lengths and point used for this
								fuselage (optional). If not specified, the global reference
								lengths and point are used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="components" type="geogenComponentsType">
						<xsd:annotation>
							<xsd:documentation>Collection of all component templates to be
								assembled as a fuselage. Allowed are definitions of components
								which are valid subcomponents of the componentType 'Fuselage',
								as specified in the used component catalog. Refer to the
								documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parameters" type="geogenParametersType">
						<xsd:annotation>
							<xsd:documentation>Optional parameters for the componentType
								'Fuselage', as specified in the used component catalog. Refer to
								the documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outputOptions" type="geogenFuselageOutputOptionsType">
						<xsd:annotation>
							<xsd:documentation>Output options for this fuselage.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenFuselagesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenFuselagesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Elements of type geogenFuselagesType contain
							definitions of fuselage assemblies which should be generated by
							GeoGen.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="fuselage" type="geogenFuselageType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenOutputOptionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenOutputOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenOutputOptionsType contains all global output
							options for GeoGen.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="destinationModelUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the aircraft or rotorcraft model to
								which the generated fuselages/wings/rotors should be added
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Default number of sections for all generated
								fuselages/wings/rotorblades</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Default number of points per section for all
								generated fuselages/wings/rotorblades</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenParameterConstraintType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenParameterConstraintType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenParameterConstraintType is used to set upper
							and lower bounds for GeoGen parameters.</ddue:para>
						<ddue:para>The value can be any valid string for the parameter
							type defined in the component catalog.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="stringBaseType">
				<xsd:attribute default="1" name="inclusive" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Indicates whether the the boundary is included
							in the valid range of the parameter ('&lt;=', '&gt;=') or not
							('&lt;', '&gt;').</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenParameterType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenParameterType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenParameterType contains the definition of a
							GeoGen parameter.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="catalogParameterUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the parameter as defined in the
								component catalog used by GeoGen.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="type" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Parameter type can be 'fixed'(parameter is set
								to a fixed value) or 'free'(parameter value is set by GeoGen
								during the internal optimization process)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="value" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Actual value of the parameter
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="min" type="geogenParameterConstraintType">
						<xsd:annotation>
							<xsd:documentation>Lower boundary value for the parameter. Only
								used during the internal optimization of parameters of type
								'free'</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="max" type="geogenParameterConstraintType">
						<xsd:annotation>
							<xsd:documentation>Upper boundary value for the parameter. Only
								used during the internal optimization of parameters of type
								'free'</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="initialValue" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Initial value of the parameter. Only used
								during the internal optimization of parameters of type 'free'
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="step" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Initial step for the optimization of the
								parameter. Only used for parameters of type 'free'
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>An optional unique ID that can be used as a
							reference to the parameter</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenParametersType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenParametersType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenParametersType contains a collection of
							GeoGen parameter definitions.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="parameter" type="geogenParameterType">
						<xsd:annotation>
							<xsd:documentation>GeoGen parameter definition
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenReferenceType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenReferenceType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenReferenceType is used to define the reference
							lengths and the reference point used by Geogen.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="length" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference length in [m] used to dimensionalize
								all parameters of type 'length', which do not include a unit in
								their &lt;value&gt; element. GeoGen uses the default value 1 [m]
								if the element is not specified or if it contains invalid data.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="width" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference length in [m] used to dimensionalize
								all parameters of type 'width', which do not include a unit in
								their &lt;value&gt; element. GeoGen uses the default value 1 [m]
								if the element is not specified or if it contains invalid data.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="height" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference length in [m] used to dimensionalize
								all parameters of type 'height', which do not include a unit in
								their &lt;value&gt; element. GeoGen uses the default value 1 [m]
								if the element is not specified or if it contains invalid data.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="area" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference area in [m2] used to dimensionalize
								all parameters of type 'area', which do not include a unit in
								their &lt;value&gt; element. GeoGen uses the default value 1
								[m2] if the element is not specified or if it contains invalid
								data.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="point" type="pointType">
						<xsd:annotation>
							<xsd:documentation>Reference point used by GeoGen to position the
								component assemblies in the global coordinate system. GeoGen
								uses the default (0; 0; 0) if the element is not specified or if
								it contains invalid data.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenRotorBladeOutputOptionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenRotorBladeOutputOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenRotorBladeOutputOptionsType contains options
							used during the export of a rotorBlade from CATIA to CPACS.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="destinationRotorBladeUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Destination UID of the rotorBlade which will
								be written into the CPACS file.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="destinationRotorUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Destination UID of the rotor to which the
								rotorBlade will be attached.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="sectionDistributionMode">
						<xsd:annotation>
							<xsd:documentation>Section distribution mode. If not specified,
								the value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="automatic"/>
									<xsd:enumeration value="distributionLawEquidistant"/>
									<xsd:enumeration value="distributionLawFullCosine"/>
									<xsd:enumeration value="distributionLawHalfCosineFront"/>
									<xsd:enumeration value="distributionLawHalfCosineRear"/>
									<xsd:enumeration value="manual"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of sections. If not specified, the
								value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of points per section. If not
								specified, the value specified in the global output options is
								used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenRotorBladeType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenRotorBladeType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenRotorBladeType contains all information
							needed by GeoGen to assemble component templates as a rotorBlade
							and to export the geometry to CPACS.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="reference" type="geogenReferenceType">
						<xsd:annotation>
							<xsd:documentation>Reference lengths and point used for this
								rotorBlade (optional). If not specified, the global reference
								lengths and point are used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="transformation" type="transformationType"/>
					<xsd:element name="components" type="geogenComponentsType">
						<xsd:annotation>
							<xsd:documentation>Collection of all component templates to be
								assembled as a rotorBlade. Allowed are definitions of components
								which are valid subcomponents of the componentType 'RotorBlade',
								as specified in the used component catalog. Refer to the
								documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parameters" type="geogenParametersType">
						<xsd:annotation>
							<xsd:documentation>Optional parameters for the componentType
								'RotorBlade', as specified in the used component catalog. Refer
								to the documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outputOptions" type="geogenRotorBladeOutputOptionsType">
						<xsd:annotation>
							<xsd:documentation>Output options for this rotorBlade.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenRotorBladesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenRotorBladesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Elements of type geogenRotorBladesType contain
							definitions of rotorBlades which should be generated by GeoGen.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="rotorBlade" type="geogenRotorBladeType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenToolType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenToolType contains all toolspecific input data
							required by the tool GeoGen.</ddue:para>
						<ddue:para>GeoGen is a geometry generation tool based on the
							commertial CAD application CATIA V5. It generates fuselage, wing
							and rotor definitions by assembling predifined fully parametric
							component templates in CATIA and exporting the resulting
							geometries into CPACS.</ddue:para>
						<ddue:para>GeoGen is being developed at the DLR Institute for
							Aerodynamics and Flow Technology (AS) in terms of the project
							RIDE (Rotorcraft Integrated Design and Evaluation).</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="assembly" type="geogenAssemblyType">
						<xsd:annotation>
							<xsd:documentation>Definition of the fuselage, wing and rotor
								components to be created by GeoGen</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="attachedMassPositions" type="geogenAttachedMassPositionsType">
						<xsd:annotation>
							<xsd:documentation>Definition of mass positions using GeoGen
								point components.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outputOptions" type="geogenOutputOptionsType">
						<xsd:annotation>
							<xsd:documentation>Global output options for GeoGen
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenWingOutputOptionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenWingOutputOptionsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenWingOutputOptionsType contains options used
							during the export of a wing from CATIA to CPACS.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="destinationWingUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Destination UID of the wing which will be
								written into the CPACS file.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of part to which the wing is mounted (if
								any). The parent of the wing can e.g. be the fuselage. In each
								aircraft model, there is exactly one part without a parent part
								(The root of the connection hierarchy).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="symmetry">
						<xsd:annotation>
							<xsd:documentation>If there exists a mirrored counterpart of the
								wing: specify the symmetry plane here</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="x-y-plane"/>
									<xsd:enumeration value="x-z-plane"/>
									<xsd:enumeration value="y-z-plane"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="sectionDistributionMode">
						<xsd:annotation>
							<xsd:documentation>Section distribution mode. If not specified,
								the value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="automatic"/>
									<xsd:enumeration value="distributionLawEquidistant"/>
									<xsd:enumeration value="distributionLawFullCosine"/>
									<xsd:enumeration value="distributionLawHalfCosineFront"/>
									<xsd:enumeration value="distributionLawHalfCosineRear"/>
									<xsd:enumeration value="manual"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of sections. If not specified, the
								value specified in the global output options is used.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xsd:annotation>
							<xsd:documentation>Number of points per section. If not
								specified, the value specified in the global output options is
								used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenWingType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenWingType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The geogenWingType contains all information needed by
							GeoGen to assemble component templates as a wing and to export
							the geometry to CPACS.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="reference" type="geogenReferenceType">
						<xsd:annotation>
							<xsd:documentation>Reference lengths and point used for this wing
								(optional). If not specified, the global reference lengths and
								point are used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="transformation" type="transformationType"/>
					<xsd:element name="components" type="geogenComponentsType">
						<xsd:annotation>
							<xsd:documentation>Collection of all component templates to be
								assembled as a wing. Allowed are definitions of components which
								are valid subcomponents of the componentType 'Wing', as
								specified in the used component catalog. Refer to the
								documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="parameters" type="geogenParametersType">
						<xsd:annotation>
							<xsd:documentation>Optional parameters for the componentType
								'Wing', as specified in the used component catalog. Refer to the
								documentation of the used component catalog for further
								information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outputOptions" type="geogenWingOutputOptionsType">
						<xsd:annotation>
							<xsd:documentation>Output options for this wing.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geogenWingsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geogenWingsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Elements of type geogenWingsType contain definitions of
							wing assemblies which should be generated by GeoGen.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="wing" type="geogenWingType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geographicPointConstraintType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geographicPointConstraintType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Geographic point constraint, containing a longitude, latitude, altitude data triplet.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="longitude" type="doubleConstraintBaseType">
						<xsd:annotation>
							<xsd:documentation>Longitude coordinate 0-360</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="latitude" type="doubleConstraintBaseType">
						<xsd:annotation>
							<xsd:documentation>Latitude coordinate 0-360</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="altitude" type="doubleConstraintBaseType">
						<xsd:annotation>
							<xsd:documentation>Altitude in meters</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="geographicPointType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>geographicPointType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Geographic point type, containing a longitude, latitude, altitude data triplet.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="longitude" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Longitude coordinate 0-360</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="latitude" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Latitude coordinate 0-360</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="altitude" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Altitude in meters</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="globalAeroPerformanceType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>airfoilAeroPerformanceType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>airfoilAeroPerformance type, containing perfomance maps
							with aerodynamic data of an airfoil.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="reference" type="referenceType">
						<xsd:annotation>
							<xsd:documentation>References used for the calculation of the
								force and moment coefficients</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aeroPerformanceMaps" type="aeroPerformanceMapsType">
						<xsd:annotation>
							<xsd:documentation>Calculated aerodynamic performance maps of the
								full configuration</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="globalBeamPropertiesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>globalBeamPropertiesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>globalBeamPropertiesType, containing the global beam
							properties such as EA, EI, mass</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="source" type="stringBaseType"/>
					<xsd:element name="consistancy" type="booleanBaseType"/>
					<xsd:element name="beamCrossSection" type="beamCrossSectionType"/>
					<xsd:element name="beamCOG" type="pointXYType"/>
					<xsd:element minOccurs="0" name="beamShearCenter" type="pointXYType"/>
					<xsd:element minOccurs="0" name="beamStiffness" type="beamStiffnessType"/>
					<xsd:element minOccurs="0" name="beamSpecificMass" type="doubleBaseType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="groundLoadCaseType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>flightLoadCaseType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>Main class</ddue:title>
								<ddue:content>
									<ddue:para>
										<ddue:legacyBold>State</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Includes general definition of aircraft attitude and
										the aircraft environment.</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Aircraft Settings</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Include definition of settings of the aircraft self.
										For example setting of a flap or landinggear.</ddue:para>
									<ddue:para>
										<ddue:legacyBold>MassCase</ddue:legacyBold>
									</ddue:para>
									<ddue:para>Include definition about the massCase and is linked
										by an UID to the massBreakdown, weightAndBalance or direct
										component link. ?!</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>References</ddue:title>
								<ddue:content>
									<ddue:para>
										<ddue:legacyBold>Coordinate System(s)</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>Earth Fix coord.sys. (EF) with planar surface
												(x-y-Ebene = Earth surface, x North, y East)</ddue:listItem>
										</ddue:list>
										<ddue:para>
											<ddue:mediaLink>
												<ddue:image xlink:href="earthFixCoordSys"/>
											</ddue:mediaLink>
										</ddue:para>
										<ddue:list class="bullet">
											<ddue:listItem>refCoordUid (e.g. body fix and orientation to
												EF)</ddue:listItem>
											<ddue:listItem>refpointUid</ddue:listItem>
										</ddue:list>
										<ddue:para>
											<ddue:mediaLink>
												<ddue:image xlink:href="bodyFixCoordSys2"/>
											</ddue:mediaLink>
										</ddue:para>
									</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Euler angles</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>Attitude of body fix to earth fix coord.sys.
											</ddue:listItem>
											<ddue:listItem>ψ psi – heading</ddue:listItem>
											<ddue:listItem>θ theta – pitch angle</ddue:listItem>
											<ddue:listItem>Φ phi – bank angle</ddue:listItem>
											<ddue:listItem>Rotation order: ψ, θ, Φ</ddue:listItem>
										</ddue:list>
									</ddue:para>
									<ddue:para>
										<ddue:mediaLink>
											<ddue:image xlink:href="eulerAnglesInCoordSys"/>
										</ddue:mediaLink>
									</ddue:para>
									<ddue:para>
										<ddue:legacyBold>Reference of attitude values
										</ddue:legacyBold>
										<ddue:list class="bullet">
											<ddue:listItem>x,y,z (vector to ref point in EF)
											</ddue:listItem>
											<ddue:listItem>dx,dy,dz (vector in EF)</ddue:listItem>
											<ddue:listItem>u,v,w (vector in refCoord) (body fix)
											</ddue:listItem>
											<ddue:listItem>du,dv,dw (vector in refCoordUid) (body fix)
											</ddue:listItem>
										</ddue:list>
									</ddue:para>
									<ddue:para>
										<ddue:mediaLink>
											<ddue:image xlink:href="bodyFixCoordSys"/>
										</ddue:mediaLink>
									</ddue:para>
									<ddue:para>(dx,dy,dy --&gt; Euler transformation (psi, theta,
										phi) --&gt; u,v,w)</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Load case name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Load case description (String)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="aeroDataSetUID" type="stringUIDBaseType"/>
					<xsd:element minOccurs="0" name="loadCondition" type="loadConditionType"/>
					<xsd:element name="state" type="loadCaseStateType"/>
					<xsd:element minOccurs="0" name="aircraftSettings" type="loadCaseSettingsType"/>
					<xsd:element minOccurs="0" name="mass" type="loadCaseMassType"/>
					<xsd:choice>
						<xsd:element minOccurs="0" name="nodalLoads" type="nodalLoadsType"/>
						<xsd:element minOccurs="0" name="cutLoads" type="cutLoadsType"/>
					</xsd:choice>
					<xsd:element minOccurs="0" name="externalAmifFile" type="stringBaseType"/>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="groundLoadCasesType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>groundLoadCasesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="groundLoadCase" type="groundLoadCaseType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="guideCurveProfileGeometryType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>guideCurveProfileGeometryType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>A guide curve profile is defined by a profile name, an
								optional description and a 3-dimensional relative pointlist with
								all three coordinates mandatory. For typical profiles, one of
								the coordinate vectors contains only "0" entries. All point
								coordinates are transferred to the global coordinate system.
								First and last point may, but need not to, be identical.
							</ddue:para>
							<ddue:para>The points have to be ordered in a mathematical
								positive sense.</ddue:para>
							<ddue:para>A profile can be symmetric. In that case the profile
								is interpreted as being not closed and will be closed by
								mirroring it on the symmetry plane.</ddue:para>
							<ddue:para>Curves have to go continuously over the whole wing or
								fuselage</ddue:para>
							<ddue:para>Connection of guide curves from segment to segment
							</ddue:para>
							<ddue:mediaLink>
								<ddue:image xlink:href="guideProfileWing"/>
							</ddue:mediaLink>
							<ddue:mediaLink>
								<ddue:image xlink:href="guideProfileFuselage"/>
							</ddue:mediaLink>
							<ddue:para>Please note, currently it is not possible to apply any
								means of class based transformation in the description. However,
								this may be an addition for the future. </ddue:para>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of profile</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of profile</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="pointList" type="pointListRelXYZVectorType"/>
				</xsd:sequence>
				<xsd:attribute name="symmetry">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="x-y-plane"/>
							<xsd:enumeration value="x-z-plane"/>
							<xsd:enumeration value="y-z-plane"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="guideCurveProfilesType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>guideCurveProfilesType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Guide Curve Profiles type. This type is used to
							describe guide curves that enable designers to create a geometry
							that deviates from a standard loft.
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="guideCurveProfile" type="guideCurveProfileGeometryType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>

	</xsd:complexType>
	<xsd:complexType name="guideCurveType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Guide Curve Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:para>A guide curve may be used to alter the shape of the
								outer geometry and "guide" the loft.
							</ddue:para>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of guide curve</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of guide curve</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="guideCurveProfileUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a guide curve profile
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:annotation>
							<xsd:documentation>For the first segment fromGuideCurveUID is not
								a valid entry! For the first guideCurve
								fromRelativeCircumference must be applied! fromGuideCurveUID is
								exclusive.
							</xsd:documentation>
						</xsd:annotation>
						<xsd:sequence>
							<xsd:element name="fromGuideCurveUID" type="stringUIDBaseType">
								<xsd:annotation>
									<xsd:documentation>Reference to the previous guide curve from
										which this guide curve shall start.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="continuity">
								<xsd:annotation>
									<xsd:documentation>Continuity definition for geometry
										generation. Possible options: C0, C1 from previous, C2 from
										previous, C1 to previous, C2 to previous
									</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:simpleContent>
										<xsd:restriction base="stringBaseType">
											<xsd:enumeration value="C0"/>
											<xsd:enumeration value="C1 from previous"/>
											<xsd:enumeration value="C2 from previous"/>
											<xsd:enumeration value="C1 to previous"/>
											<xsd:enumeration value="C2 to previous"/>
										</xsd:restriction>
									</xsd:simpleContent>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element name="fromRelativeCircumference" type="doubleBaseType">
								<xsd:annotation>
									<xsd:documentation>Reference to the relative circumference
										position from which the guide curve shall start. Valid values
									    are in the interval -1.0...1.0.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element minOccurs="0" name="tangent" type="pointXYZType">
								<xsd:annotation>
									<xsd:documentation>Tangent at first point</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:choice>
					<xsd:sequence>
						<xsd:element name="toRelativeCircumference" type="doubleBaseType">
							<xsd:annotation>
								<xsd:documentation>The relative circumference
									position at which the guide curve shall end. Valid values
									are in the interval -1.0...1.0.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="tangent" type="pointXYZType">
							<xsd:annotation>
								<xsd:documentation>Tangent at last point</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:element minOccurs="0" name="rXDirection" type="pointXYZType">
							<xsd:annotation>
									<xsd:documentation>Local direction along which the relative x-coordinates of
									the guide curve points are defined. For the wing the default is
									the wing's local x-axis, for the fuselage its the fuselage's local z-axis.
									</xsd:documentation>
							</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>

	</xsd:complexType>

	<xsd:complexType name="guideCurvesType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Guide Curves Type</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Guide Curve type. This type is used to describe guide
							curves that enable designers to create a geometry that deviates
							from a standard loft.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="guideCurve" type="guideCurveType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>

	</xsd:complexType>
	<xsd:complexType name="handbookAeroControlSurfaceTotalCoefficientsFormulasType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Formulas for modification of the delta-performance maps
							of a control surface</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains a set of formulas for
										modification of the delta-performance maps for a control
										surface. The syntax of the formulas is specified at the base
										performance map description.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;controlSurfaceUID&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Reference to a specific control surface.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;dcfxFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta force coefficient in
										x direction.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;dcfyFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta force coefficient in
										y direction.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;dcfzFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta force coefficient in
										z direction.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;dcmxFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta moment coefficient
										around the x axis.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;dcmyFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta moment coefficient
										around the y axis.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>7.
									&lt;dcmzFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying the delta moment coefficient
										around the z axis.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="controlSurfaceUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a control surface
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfxFormula" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="dcfyFormula" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="dcfzFormula" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="dcmxFormula" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="dcmyFormula" type="stringBaseType"/>
					<xsd:element minOccurs="0" name="dcmzFormula" type="stringBaseType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroControlSurfacesTotalCoefficientsFormulasType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>List of control surfaces</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains a list of control surfaces, for
										which the delta performance maps shall be modified by formula.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;controlSurface&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Control surface for which modification formulas are
										specified.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="controlSurface" type="handbookAeroControlSurfaceTotalCoefficientsFormulasType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroDampingDerivativesFormulasType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Formulas for modifying performane maps with damping
							derivatives</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains formulas for modifying the
										aerodynamic performance maps with the damping derivatives. The
										derivatives are calculated using rotational rates [rad/s],
										normalized by: Rate*ReferenceLength/flow speed. The rotations
										are performed around the global axis directions with the
										aircraft model's global reference point as origin. The
										formulas are used in the same way as for the base performanc
										maps.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1. &lt;dcfxdpstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfxdpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2. &lt;dcfxdqstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfxdqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3. &lt;dcfxdrstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfxdrstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4. &lt;dcfydpstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfydpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5. &lt;dcfydqstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfydqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6. &lt;dcfydrstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfydrstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>7. &lt;dcfzdpstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfzdpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>8. &lt;dcfzdqstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfzdqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>9. &lt;dcfzdrstarFormula&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcfzdrstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>10. &lt;dcmxdpstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmxdpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>11. &lt;dcmxdqstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmxdqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>12. &lt;dcmxdrstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmxdrstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>13. &lt;dcmydpstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmydpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>14. &lt;dcmydqstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmydqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>15. &lt;dcmydrstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmydrstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>16. &lt;dcmzdpstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmzdpstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>17. &lt;dcmzdqstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmzdqstar</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>18. &lt;dcmzdrstarFormula&gt; (optional)
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modifying dcmzdrstar</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="dcfxdpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfxdpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfxdqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfxdqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfxdrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfxdrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfydpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfydpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfydqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfydqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfydrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfydrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfzdpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfzdpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfzdqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfzdqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcfzdrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcfzdrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmxdpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmxdpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmxdqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmxdqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmxdrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmxdrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmydpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmydpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmydqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmydqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmydrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmydrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmzdpstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmzdpstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmzdqstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmzdqstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dcmzdrstarFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for modifying dcmzdrstar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroDampingDerivativesRatesFormulasType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Formulas for damping derivatives for positive and
							negative rotation rates</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains the formulas for modification of
										the damping derivatives for the aerodynamic performance map
										computations with HandbookAero. They are split up into those
										formulas for positive rotation rates, and those for negative
										rotation rates.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1. &lt;positiveRates&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formulas for modification of damping derivatives,
										calculated by positive rotation rates.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2. &lt;negativeRates&gt; (optional)</ddue:title>
								<ddue:content>
									<ddue:para>Formulas for modification of damping derivatives,
										calculated by negative rotation rates.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="positiveRates" type="handbookAeroDampingDerivativesFormulasType"/>
					<xsd:element minOccurs="0" name="negativeRates" type="handbookAeroDampingDerivativesFormulasType"/>
				</xsd:all>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroLastTransitionRegionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Parameters and options of the last transition region.
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the options and parameters for
										the last transition region. It ranges from (including) the
										maximum value of the prvious transition region, or from 0, if
										this is the only transition region. It ranges up to a Reynolds
										number of infinity.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;laminarFormFactor&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Form factor for the laminar part of the transition
										region. If this value is not specified, the laminar form
										factor is calculated from the turbulent one. If the turbulent
										form factor is missing as well, both form factors are
										calculated automatically (individually for each wing strip).
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;turbulentFormFactor&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Form factor for the turbulent part of the transition
										region. If this value is not specified, the turbulent form
										factor is calculated from the laminar one. If the laminar form
										factor is missing as well, both form factors are calculated
										automatically (individually for each wing strip).</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;polynomialCoefficients&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>List of polynomial coefficients, used to specify the
										transition as an n-th grade polynomial of the local Reynolds
										number.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="laminarFormFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Form Factor for the laminar wing region
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="turbulentFormFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Form Factor for the turbulent wing region
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="polynomialCoefficients" type="handbookAeroPolynomialCoefficientsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroLoadCaseType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>A loadcase for computation with HandbookAero
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This type contains the whole set of operational
										parameters, required to compute a single loadcase with
										HandbookAero.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;name&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This is the name of the loadcase.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;description&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This is the the description of the loadcase.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;loadCaseUID&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This is a reference to the original loadcase
										definition within the selected aircraft model. It is used to
										locate the place, where the tool results have to be stored.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;flow&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the definition of the flow
										conditions for the aerodynamic computations.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;totalCoefficients&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>If HandbookAero is used to extend / modify existing
										loadcase data, this node has to contain the total coefficients
										for the specific loadcase.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;wings&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>If HandbookAero is used to extend / modify existing
										loadcase data, this node has to contain the aerodynamic
										coefficients for all the wings.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the loadcase</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of the loadcase
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="loadCaseUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a loadcase</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="flow" type="flowConditionType"/>
					<xsd:element minOccurs="0" name="totalCoefficients" type="coefficientsType"/>
					<xsd:element minOccurs="0" name="wings" type="wingsCoefficientsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroLoadCasesType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Date for HandbookAero loadcase computation</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>
										This type contains data for the loadcase computations in
										HandbookAero. In
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
										mode this means just a list of UIDs, referring to the selected
										loadcases. In
										<ddue:legacyItalic>Tool input</ddue:legacyItalic>
										mode, the loadcase parameters themselves are stored here,
										instead.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;loadCaseUID&gt; (mandatory in
									<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A list of loadcase UIDs, referring to each of the
										loadcases to be computed.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;loadCase&gt; (mandatory in
									<ddue:legacyItalic>Tool input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A list of the loadcases to be computed with all
										their required parameters.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:element maxOccurs="unbounded" name="loadCaseUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a loadcase</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" name="loadCase" type="handbookAeroLoadCaseType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroModifyByFormulaType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Parameters and options for the modification of
							aerodynamic data by formulas</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains methods for modification of
										aerodynamic data by formulas. Currently, it does only support
										modifications of the total coefficients.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;totalCoefficients&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Modification of total coefficients by formulas.
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:element name="totalCoefficients" type="handbookAeroTotalCoefficientsFormulasType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroPerformanceMapType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Handbook Aero performance map data</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>
										This type contains data for the aerodynamic performance map
										computations with HandbookAero. In
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
										mode no further input is required here. But it is possible to
										specify a quasiSteadyRotation node to switch on the
										computation of damping derivatives. A list of control surface
										UIDs further switches on the computation of delta-performance
										maps for each of these control surfaces. In
										<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
										mode, the parameter vectors for the performance map
										computations, and optionally the quasi steady rotation values,
										have to be specified here. If already existing performance
										maps shall be modified, they have to be specified here, as
										well.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;positiveQuasiSteadyRotation&gt; (optional in
									<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A set of normalized rotation values used for
										calculating the damping derivatives (positive rates). A
										separate computation for each of the three rotational values
										is performed and used (together with the reference computation
										without any rotation) to derive the damping derivatives. This
										node itself switches on the computation of positive damping
										derivatives. If there are no subnodes, the positive defaults
										are used.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;negativeQuasiSteadyRotation&gt; (optional in
									<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A set of normalized rotation values used for
										calculating the damping derivatives (negative rates). A
										separate computation for each of the three rotational values
										is performed and used (together with the reference computation
										without any rotation) to derive the damping derivatives. This
										node itself switches on the computation of negative damping
										derivatives. If there are no subnodes, the negative defaults
										are used.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;controlSurfaceUID&gt; (optional in
									<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A list of control surface UIDs, referring to each of
										the control surfaces for which a delta performance map shall
										be computed.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;machNumber&gt; (mandatory in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A vector of semicolon-separated values, specifying
										all the Mach numbers being used to create the aerodynamic
										performance map (Mach number dimension)</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;reynoldsNumber&gt; (mandatory in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A vector of semicolon-separated values, specifying
										all the Reynolds numbers being used to create the aerodynamic
										performance map (Reynolds number dimension)</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;angleOfYaw&gt; (mandatory in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A vector of semicolon-separated values, specifying
										all the angles of yaw being used to create the aerodynamic
										performance map (Angle of yaw dimension)</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>7.
									&lt;angleOfAttack&gt; (mandatory in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A vector of semicolon-separated values, specifying
										all the angles of attack being used to create the aerodynamic
										performance map (Angle of attack dimension)</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>8.
									&lt;cfx&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cfx values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>9.
									&lt;cfy&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cfy values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>10.
									&lt;cfz&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cfz values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>11.
									&lt;cmx&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cmx values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>12.
									&lt;cmy&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cmy values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>13.
									&lt;cmz&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance map with all the cmz values for
										modification in HandbookAero. It consists of
										semicolon-separated values, for each combination of Mach
										number, Reynolds number, angle of yaw and angle of attack. In
										the performance map, the last dimension (angle of attack) is
										varied first.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>14.
									&lt;positiveQuasiSteadyRotation&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>
										A set of normalized rotation values used for calculating the
										damping derivatives (positive rates). A separate computation
										for each of the three rotational values is performed and used
										(together with the reference compuation without any rotation)
										to derive the damping derivatives. This node itself switches
										on the computation of positive damping derivatives. If there
										are no subnodes, the positive defaults are used. This node is
										similar to the one from
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
										mode.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>15.
									&lt;negativeQuasiSteadyRotation&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>
										A set of normalized rotation values used for calculating the
										damping derivatives (negative rates). A separate computation
										for each of the three rotational values is performed and used
										(together with the reference compuation without any rotation)
										to derive the damping derivatives. This node itself switches
										on the computation of negative damping derivatives. If there
										are no subnodes, the negative defaults are used. This node is
										similar to the one from
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
										mode.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>16.
									&lt;dampingDerivatives&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>The performance maps with the damping derivatives
										for modification in HandbookAero.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>16.
									&lt;controlSurfaces&gt; (optional in
									<ddue:legacyItalic>Toolwrapper input</ddue:legacyItalic>
									mode)
								</ddue:title>
								<ddue:content>
									<ddue:para>A list of control surfaces and their deflection
										vectors being used to create the delta aerodynamic performance
										maps for each control surface (Deflection dimension). Each of
										the control surfaces can optionally contain the corresponding
										delta-performance maps for modification in HandbookAero as
										well.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:choice>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="positiveQuasiSteadyRotation" type="quasiSteadyRotationType"/>
						<xsd:element minOccurs="0" name="negativeQuasiSteadyRotation" type="quasiSteadyRotationType"/>
						<xsd:element maxOccurs="unbounded" minOccurs="0" name="controlSurfaceUID" type="stringBaseType">
							<xsd:annotation>
								<xsd:documentation>Specification of control surfaces, for which
									additional delta-polars shall be computed</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="machNumber" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>Vector with Mach Numbers for computation
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reynoldsNumber" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>Vector of Reynolds Numbers for computation
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="angleOfYaw" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>Vector of yaw angles for computation
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="angleOfAttack" type="stringVectorBaseType">
							<xsd:annotation>
								<xsd:documentation>Vector of angles of attack for computation
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="cfx" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="cfy" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="cfz" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="cmx" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="cmy" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="cmz" type="stringArrayBaseType"/>
						<xsd:element minOccurs="0" name="positiveQuasiSteadyRotation" type="quasiSteadyRotationType"/>
						<xsd:element minOccurs="0" name="negativeQuasiSteadyRotation" type="quasiSteadyRotationType"/>
						<xsd:element minOccurs="0" name="dampingDerivatives" type="dampingDerivativesRatesType"/>
						<xsd:element minOccurs="0" name="controlSurfaces" type="controlSurfacePerformanceMapsType"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroPolynomialCoefficientsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Polynomial coefficients for a specifying the transition
							location on a transition region</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the polynomial coefficients for
										the transition location.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;polynomialCoefficient&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Polynomial coefficients for the transition
										definition (1st one for 0th grade, 2nd one for 1st grade,
										...).</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="polynomialCoefficient" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Polynomial coefficients for the transition
								definition (1st one for 0th grade, 2nd one for 1st grade, ...).
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroToolParametersType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Parameters for the HandbookAero tool</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>Parameters and options for the HandbookAero tool.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;dataInsertionMode&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										Way of insertion for results. There are three options
										available:
										<ddue:list class="bullet">
											<ddue:listItem>add: Adding computed results to existing
												values</ddue:listItem>
											<ddue:listItem>subtract: Subtracting computed results from
												existing values</ddue:listItem>
											<ddue:listItem>replace: Replacing existing values by computed
												results</ddue:listItem>
										</ddue:list>
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;logLevel&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										Detail level of handbookAero logging. There are three options
										available:
										<ddue:list class="bullet">
											<ddue:listItem>0: Minimum logging</ddue:listItem>
											<ddue:listItem>1: Detailed logging</ddue:listItem>
											<ddue:listItem>2: Very detailed logging</ddue:listItem>
										</ddue:list>
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;wingPaneling&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										This node contains a parameter for the fineness of the wing
										paneling. The value is used as common value for all wings. In
										case of
										<ddue:legacyItalic>&lt;loadCases&gt;</ddue:legacyItalic>
										runmode, and if already existing spanwise distributions are
										provided, this provided paneling is used instead.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;viscousDrag&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the available methods and their
										parameters for calculating the viscous drag.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;modifyByFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the parameters for creating or
										modifying data by specified formulas.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="dataInsertionMode">
						<xsd:annotation>
							<xsd:documentation>Way of insertion for results. add=adding
								computed results to existing values subtract=subtracting
								computed results from existing values replace=replacing existing
								values by computed results</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="stringBaseType">
									<xsd:enumeration value="add"/>
									<xsd:enumeration value="subtract"/>
									<xsd:enumeration value="replace"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="logLevel">
						<xsd:annotation>
							<xsd:documentation>Detail level of handbookAero logging
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:restriction base="integerBaseType">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
								</xsd:restriction>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="wingPaneling" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Approx. [Value * 5 + Number of spanwise
								segments (including additional segments due to control
								surfaces)] spanwise panels. If the wing is symmetric, this
								values is used for one side.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="viscousDrag" type="handbookAeroViscousDragType"/>
					<xsd:element minOccurs="0" name="modifyByFormula" type="handbookAeroModifyByFormulaType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroToolType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>HandbookAero tool data</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>Toolspecific data for the HandbookAero tool.
										HandbookAero is a collection of handbook methods, that can be
										used to incorporate estimations for aerodynamic effects that
										are not covered by the simple, physics based aerodynamic
										models. HandbookAero can either be applied on a clean dataset
										(in terms of aerodynamics), or on an already existing dataset
										to extend / modify it. Currently, HandbookAero provides
										methodes for computing the skin friction drag (based on flat
										plate analogy) including a thickness correction and a formula
										based methodology to modify existing aerodynamic results.
									</ddue:para>
									<ddue:para>
										In this toolspecific data block, two ways of input definition
										are possible:
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
										and
										<ddue:legacyItalic>Tool input</ddue:legacyItalic>
										.
										<ddue:list class="bullet">
											<ddue:listItem>
												<ddue:legacyItalic>Framework integration</ddue:legacyItalic>
												is the typical way, how LIFTING_LINE is used via an
												integration framework. Reference values are taken from the
												selected aircraft model's &lt;reference&gt; node and
												operating conditions are specified as links to the
												corresponding definitions in the selected aircraft model as
												well. During the
												<ddue:legacyItalic>input mapping</ddue:legacyItalic>
												phase (which is done in framework integration before the
												tool itself is started), reference values and operating
												conditions are copied into this toolspecific block to create
												a valid
												<ddue:legacyItalic>Tool input</ddue:legacyItalic>
												dataset for the tool itself.
											</ddue:listItem>
											<ddue:listItem>
												<ddue:legacyItalic>Tool input</ddue:legacyItalic>
												is the required input dataset for the HandbookAero tool
												itself. It is either created by the integration framework
												(see above), or - if HandbookAero is used as a stand-alone
												program - it has to be provided by the user.
											</ddue:listItem>
										</ddue:list>
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;tool&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Identification of the tool. The &lt;name&gt; subnode
										is always "HandbookAero", the &lt;version&gt; subnode contains
										the used toolversion in the form "#x.#y with #x and #y as
										major and minor version numbers.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;aircraftModelUID&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Unique identifier (UID) of the aircraft model to be
										analyzed.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;datasetName&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										Name of the dataset (used during HandbookAero computation
										only). If this node is missing, the
										<ddue:legacyItalic>CPACS</ddue:legacyItalic>
										dataset name is used as default.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;referenceValues&gt;
									<ddue:legacyItalic>(Tool input only, mandatory)
									</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										Set of reference values used for HandbookAero computations.
										This node is required for the
										<ddue:legacyItalic>Tool input</ddue:legacyItalic>
										only. In
										<ddue:legacyItalic>Framework integration</ddue:legacyItalic>,
										these values are taken form the specified aircraft model's
										&lt;reference&gt; node (using &lt;length&gt; for all three
										HandbookAero reference lengths).
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									Either &lt;loadCases&gt;or &lt;performanceMaps&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>
										These nodes contain the operating conditions of the aircraft
										for the HandbookAero computations. HandbookAero distinguishes
										between two runmodes:
										<ddue:legacyItalic>&lt;loadCases&gt;</ddue:legacyItalic>
										and
										<ddue:legacyItalic>&lt;performanceMaps&gt;</ddue:legacyItalic>
										. In
										<ddue:legacyItalic>&lt;loadCases&gt;</ddue:legacyItalic>
										mode, a set of individual loadcases is computed. In
										<ddue:legacyItalic>&lt;performanceMaps&gt;</ddue:legacyItalic>
										mode, an aerodynamic performance map over a sweep of Mach- and
										Reynolds-numbers, angles of yaw and of attack is created or
										modified (optionally extended by delta-performance maps for
										control surfaces).
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;toolParameters&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>In this node, all the control parameters for
										HandbookAero are specified.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to aircraft model
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="datasetName" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of the dataset for HandbookAero
								calculation</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="referenceValues" type="aeroToolReferenceType"/>
					<xsd:choice>
						<xsd:element name="loadCases" type="handbookAeroLoadCasesType"/>
						<xsd:element name="performanceMap" type="handbookAeroPerformanceMapType"/>
					</xsd:choice>
					<xsd:element name="toolParameters" type="handbookAeroToolParametersType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroTotalCoefficientsFormulasType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Formulas for modification of aerodynamic data.
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This
										node contains the formulas for modification of the aerodynamic
										coefficients. They are formulated in the way: coefficient =
										[coefficientFormula], e.g.: &lt;cfzFormula&gt;="cfz + 0.1"
										means "cfz = cfz + 0.1". The formulas are either modifying the
										total results of discrete loadcases, or they are modifying the
										whole performance map for that coefficient. Available formula
										operators are (from low to high priority):
										<ddue:table>
											<ddue:row>
												<ddue:entry>
													<ddue:legacyBold>Operator</ddue:legacyBold>
												</ddue:entry>
												<ddue:entry>
													<ddue:legacyBold>Explanation</ddue:legacyBold>
												</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>+</ddue:entry>
												<ddue:entry>Addition</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>-</ddue:entry>
												<ddue:entry>Subtraction</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>*</ddue:entry>
												<ddue:entry>Multiplication</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>/</ddue:entry>
												<ddue:entry>Division</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>^</ddue:entry>
												<ddue:entry>Raise to power</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>SQRT()</ddue:entry>
												<ddue:entry>Square root</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>ARCSIN()</ddue:entry>
												<ddue:entry>Arc sine</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>SIN()</ddue:entry>
												<ddue:entry>Sine</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>ARCCOS()</ddue:entry>
												<ddue:entry>Arc cosine</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>COS()</ddue:entry>
												<ddue:entry>Cosine</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>ARCTAN()</ddue:entry>
												<ddue:entry>Arc tangent</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>TAN()</ddue:entry>
												<ddue:entry>Tangent</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>EXP()</ddue:entry>
												<ddue:entry>Exponential function (base-e)</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>LN()</ddue:entry>
												<ddue:entry>Natural logarithm (base-e)</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>LOG()</ddue:entry>
												<ddue:entry>Common logarithm (base-10)</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>()</ddue:entry>
												<ddue:entry>Brackets (can be nested to a nearly unlimited
													depth)</ddue:entry>
											</ddue:row>
										</ddue:table>
										The formula parser is working case-insensitive and ignores
										whitespaces. Numbers can be written in decimal or exponential
										form (examples (separated by semicolons): 5 3. 7.3 5.7e2),
										using "." as decimal character. The following parameters can
										be used:
										<ddue:table>
											<ddue:row>
												<ddue:entry>
													<ddue:legacyBold>Parameter</ddue:legacyBold>
												</ddue:entry>
												<ddue:entry>
													<ddue:legacyBold>Explanation</ddue:legacyBold>
												</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>MA</ddue:entry>
												<ddue:entry>Mach number</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>RE</ddue:entry>
												<ddue:entry>Reynolds number</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>AOY</ddue:entry>
												<ddue:entry>Angle of yaw</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>AOA</ddue:entry>
												<ddue:entry>Angle of attack</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CFX</ddue:entry>
												<ddue:entry>Force coefficient in x direction</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CFY</ddue:entry>
												<ddue:entry>Force coefficient in y direction</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CFZ</ddue:entry>
												<ddue:entry>Force coefficient in z direction</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CMX</ddue:entry>
												<ddue:entry>Moment coefficient around the x axis
												</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CMY</ddue:entry>
												<ddue:entry>Moment coefficient around the y axis
												</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>CMZ</ddue:entry>
												<ddue:entry>Moment coefficient around the z axis
												</ddue:entry>
											</ddue:row>
											<ddue:row>
												<ddue:entry>PI</ddue:entry>
												<ddue:entry>PI constant</ddue:entry>
											</ddue:row>
										</ddue:table>
										The coefficient formulas are evaluated in such order, that the
										coefficients, which are used as parameters, are recalculated
										first. If, e.g., &lt;cfxFormula&gt; would be "CFZ^2/(PI*10)",
										then &lt;cfzFormula&gt; would be evaluated first.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;cfxFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of force coefficients in x
										direction</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;cfyFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of force coefficients in y
										direction</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;cfzFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of force coefficients in z
										direction</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;cmxFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of moment coefficients
										around the x axis</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>5.
									&lt;cmyFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of moment coefficients
										around the y axis</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>6.
									&lt;cmzFormula&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of moment coefficients
										around the z axis</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>7.
									&lt;dampingDerivatives&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of the damping derivatives.
										This node can only be used in case of performance map
										computations.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>8.
									&lt;controlSurfaces&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Formula for modification of the control surface
										performance maps. This node can only be used in case of
										performance map computations.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="cfxFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the force coefficient in x
								direction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfyFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the force coefficient in y
								direction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cfzFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the force coefficient in z
								direction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmxFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the moment coefficient around the
								x axis</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmyFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the moment coefficient around the
								y axis</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="cmzFormula" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Formula for the moment coefficient around the
								z axis</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="dampingDerivatives" type="handbookAeroDampingDerivativesRatesFormulasType"/>
					<xsd:element minOccurs="0" name="controlSurfaces" type="handbookAeroControlSurfacesTotalCoefficientsFormulasType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroTransitionRegionType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>The parameters for a transition region.</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>This node contains the options and parameters for a
										transition region. It is used only, if more than one region is
										defined.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;maxReynoldsNumber&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Upper limit of the RE-Region (less than this value).
										The region ranges from (including it) the end of previous
										region (or 0 if this is the first region) up to this value).
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;laminarFormFactor&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Form factor for the laminar part of the transition
										region. If this value is not specified, the laminar form
										factor is calculated from the turbulent one. If the turbulent
										form factor is missing as well, both form factors are
										calculated automatically (individually for each wing strip).
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;turbulentFormFactor&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Form factor for the turbulent part of the transition
										region. If this value is not specified, the turbulent form
										factor is calculated from the laminar one. If the laminar form
										factor is missing as well, both form factors are calculated
										automatically (individually for each wing strip).</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>4.
									&lt;polynomialCoefficients&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>List of polynomial coefficients, used to specify the
										transition as an n-th grade polynomial of the local Reynolds
										number.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="maxReynoldsNumber" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Upper limit of the RE-Region (Region ranges
								from (including) end of previous region (or 0) to specified
								(less than) value).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="laminarFormFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Form Factor for the laminar wing region
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="turbulentFormFactor" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Form Factor for the turbulent wing region
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="polynomialCoefficients" type="handbookAeroPolynomialCoefficientsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroTransitionRegionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Definition of the parameters for the transition regions
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>A list of transition regions. Each region contains
										the parameters for viscous drag computation in that region.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;transitionRegion&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>If more than one transition region is defined, all
										further regions are specified in this node. It contains the
										parameters for transition as well as the corresponding form
										factors.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;lastTransitionRegion&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>At least one transition region is mandatory. This
										required region is defined here. It contains the parameters
										for transition as well as the corresponding form factors.
									</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="transitionRegion" type="handbookAeroTransitionRegionType"/>
					<xsd:element name="lastTransitionRegion" type="handbookAeroLastTransitionRegionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroViscousDrag1Type">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>Method "DLR1 for calculating viscous drag</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>Calculation of viscous drag by using a flat plate
										analogy to compute skin friction drag, combined with form
										factors for additional pressure drag due to viscous
										separation. The flat plate is computed including
										laminar-turbulent transition, with the transition position as
										polynomial function of the local Reynolds number.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;wings&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This node contains a list of wings and theit
										parameters for computation with this method.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="wings" type="handbookAeroWingsTransitionsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroViscousDragType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>List of available methods for computing viscous drag
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>Available methods for viscous drag computation.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;methodDLR1&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>Calculation of viscous drag by using a flat plate
										analogy to compute skin friction drag, combined with form
										factors for additional pressure drag due to viscous
										separation. The flat plate is computed including
										laminar-turbulent transition, with the transition position as
										polynomial function of the local Reynolds number.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="methodDLR1" type="handbookAeroViscousDrag1Type"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroWingTransitionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>A selected wing with its transition definition
						</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>Parameters for calculating the viscous drag of a
										wing, or (if common parameters shall be used) for all wings.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;wingUID&gt;
									<ddue:legacyItalic>(optional)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>This node contains a reference to the wing, for
										which the viscous drag shall be calculated. If all wings shall
										be computed with common parameters, this node can be omitted.
									</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>2.
									&lt;upperSideTransitionRegions&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>In this subnode, the tansition regions and
										parameters for this region are specified for the upper side of
										the wing.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>3.
									&lt;lowerSideTransitionRegions&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>In this subnode, the tansition regions and
										parameters for this region are specified for the lower side of
										the wing.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element minOccurs="0" name="wingUID" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Reference to a wing; required, if separate
								transition definitions for each wing are used. If missing,
								transition definitions are the same for all wings. If used, only
								the specified wings are calculated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="upperSideTransitionRegions" type="handbookAeroTransitionRegionsType"/>
					<xsd:element name="lowerSideTransitionRegions" type="handbookAeroTransitionRegionsType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="handbookAeroWingsTransitionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>A list of wings for computation</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:content>
							<ddue:section>
								<ddue:title>0. General overview</ddue:title>
								<ddue:content>
									<ddue:para>A list of wings and their parameters for computation
										of viscous drag. if only some wings are specified here, only
										the specified wings will be computed. It is also possible to
										specify only one wing here and to omit the reference to a wing
										UID in this wing's subnode. Then, all wings are computed with
										the specified parameters as common values.</ddue:para>
								</ddue:content>
							</ddue:section>
							<ddue:section>
								<ddue:title>1.
									&lt;wing&gt;
									<ddue:legacyItalic>(mandatory)</ddue:legacyItalic>
								</ddue:title>
								<ddue:content>
									<ddue:para>A wing which is selected for computation, including
										the required parameters.</ddue:para>
								</ddue:content>
							</ddue:section>
						</ddue:content>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="wing" type="handbookAeroWingTransitionsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="haremToolType">
		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>haremToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="tool" type="toolType">
					</xsd:element>
					<xsd:element name="aircraftModelUID" type="stringUIDBaseType">
					</xsd:element>
					<xsd:element name="flyingQualitiesUID" type="stringUIDBaseType">
					</xsd:element>
					<xsd:element name="xpil" type="doubleBaseType">
					</xsd:element>
					<xsd:element name="Vmin" type="doubleBaseType">
					</xsd:element>
					<xsd:element name="Vmax" type="doubleBaseType">
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="headerType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>headerType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Header type, containing CPACS dataset description
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="name" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Name of CPACS dataset</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="description" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Description of CPACS dataset
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="creator" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Creator of initial CPACS dataset
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="timestamp" type="dateTimeBaseType">
						<xsd:annotation>
							<xsd:documentation>Timestamp of initial CPACS dataset creation
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="version" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>Version of initial CPACS dataset
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="cpacsVersion" type="stringBaseType">
						<xsd:annotation>
							<xsd:documentation>CPACS version that the dataset is valid to
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="updates" type="updatesType"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="hingeMomentsMapType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>hingeMomentsMapType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>hingeMomentsMap type, containing a hinge moments map
							with aerodynamic data. Array order is: angleOfAttack min-&gt;max
							then angleOfSideslip then reynoldsNumber then machNumber.</ddue:para>
						<ddue:para>All coefficients in the aeroperformanceMap relate to
							the CPACS coordinate system. See documentation of the
							CPACS-Element for further information.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="machNumber" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Mach number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="reynoldsNumber" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Reynolds Number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="angleOfSideslip" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Sideslip angle</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="angleOfAttack" type="stringVectorBaseType">
						<xsd:annotation>
							<xsd:documentation>Angle of attack</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="controlSurfaces" type="controlSurfaceHingeMomentMapsType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="hostTempType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>hostTempType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The hostTempType is used to temporarily store HOST
							inputs and outputs.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="requiredPower" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Required power in [W] calculated by HOST. The
								HOST wrapper can only write this value if the node already
								exists in the CPACS input file. So make shure to create this
								node before running the HOST wrapper.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="hostToolType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>hostToolType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>The hostToolType contains all toolspecific input data
							required by the tool host.</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:all>
					<xsd:element name="tool" type="toolType">
						<xsd:annotation>
							<xsd:documentation>Tool identification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TEMP" type="hostTempType">
						<xsd:annotation>
							<xsd:documentation>Temporary inputs and outputs of HOST.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="htpFwdInterfaceDefType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>htpFwdInterfaceDefType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of the interface of forward HTP attachment
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Definition of the forward HTP attachment
							interface</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="htpFwdReinfRelWidth" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>relative width of reinforcement at fwd HTP
								attachment, between 0.0 and 1.0</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFwdPlateWidth" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>relative width of plate at fwd HTP attachment
								(only applicable for Type1 model), between 0.0 and 1.0, smaller
								than htpPlateWidth</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFwdPlateXElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of panel element at HTP forward attachment
								in x-direction (shell elements)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFwdPlateZElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of panel element at HTP forward attachment
								in z-direction (shell elements)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFwdPlateZReinfStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of reinforcements for panel element at HTP
								forward attachment in z-direction (beam elements)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFwdAttachmentStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the element to fix HTP to fuselage
								(beam elements)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="htpInterfaceDefType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>htpInterfaceDefType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Definition of the interface of HTP</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Definition of the HTP interface
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="htpFrame1UID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the fuselage frame at the forward HTP
								attchment
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpFrame2UID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of the fuselage frame at the backward HTP
								attchment
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpAlphaUp" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>maximum HTP deflection (nose up in
								degrees)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpAlphaDown" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>maximum HTP deflection (nose down in
								degrees)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpReinfAngle" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>angle of the reinforcements at backward HTP
								attchment
								(in degrees)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="htpAreaToleranceX" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Defines area (absolute) in x-direction around
								htpFrame2UID where the HTP attachmentpoint has correct position
								==&gt; check and potentially warning message</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="htpAreaToleranceY" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Defines area (absolute) in y-direction around
								the
								outer edge of htpFrame2UID where the HTP attachmentpoint has correct
								y-position ==&gt; check and potentially warning
								message
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="htpAreaToleranceZ" type="doubleBaseType">
						<xsd:annotation>
							<xsd:documentation>Defines allowed z-position for rear HTP
								attachment
								relativ to total frame height ==&gt; check and potentially warning
								message ==&gt; check and potentially warning
								message
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpStructuralElementDefinitions" type="htpStructElemDefType">
						<xsd:annotation>
							<xsd:documentation>Definition of HTP structural
								elements
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpForwardInterfaceDefinitions" type="htpFwdInterfaceDefType">
						<xsd:annotation>
							<xsd:documentation>Definition of HTP forward attachment to
								structure
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="htpStructElemDefType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>htpStructElemDefType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>definition of structural elements in HTP attachment
						</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Definition of tailplane attachment area
							(Standard Configuration)</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="htpCrossbeamsFrontStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for HTP front
								crossbeams</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpCrossbeamsRearStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for HTP rear
								crossbeams</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpDiagonalBeamsStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for HTP diagonal
								beams</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpSideBeamsStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for HTP side beams
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="htpCutoutReinfUpperStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for upper HTP cutout
								reinforcement beams, also used for lower cutout reinforcement,
								when not explicitely defined</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="htpCutoutReinfLowerStructElemUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>UID of structural element for lower HTP cutout
								reinforcement beams (optional)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="indirectOperatingCostType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>indirectOperatingCostType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="costDescription" type="genericCostType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>



	<xsd:complexType name="integerBaseType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>integerBaseType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para>Base type for integer nodes (including external data
							attributes)</ddue:para>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:integer">
				<xsd:attribute name="externalDataNodePath" type="xsd:string"/>
				<xsd:attribute name="externalFileName" type="xsd:string"/>
				<xsd:attribute name="externalDataDirectory" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

<xsd:complexType name="interConnectionStrutAttachmentType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>interConnectionStrutAttachmentType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="position" type="etaXsiRelHeightPointType">
						<xsd:annotation>
							<xsd:documentation>Definition of the position of the attachment
								joint in relative coordinates.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="material" type="materialDefinitionType">
						<xsd:annotation>
							<xsd:documentation>Material settings of the attachment.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="interconnectionStrutType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>interconnectionStrutType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element name="toControlSurfaceUID" type="stringUIDBaseType">
						<xsd:annotation>
							<xsd:documentation>uID of control surface where this flap is
								attached to by the interconnection strut.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="material" type="materialDefinitionType">
						<xsd:annotation>
							<xsd:documentation>Material settings of the strut (if strut is
								moddeled as a simple strut).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fromAttachment" type="interConnectionStrutAttachmentType">
						<xsd:annotation>
							<xsd:documentation>Definition of the attachment on this control
								surface.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="toAttachment" type="interConnectionStrutAttachmentType">
						<xsd:annotation>
							<xsd:documentation>Definition of the attachment on the other
								control surface</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="freePath" type="freePathType">
						<xsd:annotation>
							<xsd:documentation>Free path in positive (tensil) and negative
								(compression) direction before interconnection strut blocks.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="uID" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="interconnectionStrutsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>interconnectionStrutsType</ddue:para>
					</ddue:summary>
					<ddue:remarks>
						<ddue:para/>
					</ddue:remarks>
				</sd:schemaDoc>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="complexBaseType">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="interconnectionStrut" type="interconnectionStrutType">
						<xsd:annotation>
							<xsd:documentation>Definition of one interconnection strut.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="intercostalPositionType">
    <xsd:annotation>
        <xsd:appinfo>
            <sd:schemaDoc>
                <ddue:summary>
                    <ddue:para>intercostalPositionType</ddue:para>
                </ddue:summary>
                <ddue:remarks>
                    <ddue:para>intercostalPosition type, containing the position of intercostals
                        in DSS</ddue:para>
                </ddue:remarks>
            </sd:schemaDoc>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
        <xsd:extension base="complexBaseType">
            <xsd:all>
                <xsd:element minOccurs="0" name="name" type="stringBaseType"/>
                <xsd:element minOccurs="0" name="description" type="stringBaseType"/>
                <xsd:element name="frontFrameUID" type="stringUIDBaseType">
                    <xsd:annotation>
                        <xsd:documentation>UID of the frame at which intercostal
                            starts</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="rearFrameUID" type="stringUIDBaseType">
                    <xsd:annotation>
                        <xsd:documentation>UID of the forward door frame</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="doorUID" type="stringUIDBaseType">
                    <xsd:annotation>
                        <xsd:documentation>UID of the door</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="positionRelZ" type="doubleBaseType">
                    <xsd:annotation>
                        <xsd:documentation>non-dimensional value ranging between 0 and 1
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="structuralElementUID" type="stringUIDBaseType">
                    <xsd:annotation>
                        <xsd:documentation>UID of profileBasedStructuralElement used for
                            intercostal</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="uID" type="xsd:string" use="required"/>
        </xsd:extension>
    </xsd:complexContent>
</xsd:complexType>


	<xsd:complexType name="intercostalsAssemblyType">
    <xsd:annotation>
        <xsd:appinfo>
            <sd:schemaDoc>
                <ddue:summary>
                    <ddue:para>IntercostalsAssemblyType</ddue:para>
                </ddue:summary>
                <ddue:remarks>
                    <ddue:para>IntercostalsAssembly type, containing intercostal
                        definitions</ddue:para>
                </ddue:remarks>
            </sd:schemaDoc>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
        <xsd:extension base="complexBaseType">
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="intercostal" type="intercostalPositionType"/>
            </xsd:sequence>
        </xsd:extension>
    </xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="interfaceDefinitionsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>structuralElementsConnectionsType</ddue:para>
