	<xsd:complexType name="fuselageElementsType">

		<xsd:annotation>
			<xsd:appinfo>
				<sd:schemaDoc>
					<ddue:summary>
						<ddue:para>fuselageElementsType</ddue:para>
					</ddue:summary>
