<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:htm="http://www.w3.org/1999/xhtml" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:annotation>
		<xsd:documentation>
